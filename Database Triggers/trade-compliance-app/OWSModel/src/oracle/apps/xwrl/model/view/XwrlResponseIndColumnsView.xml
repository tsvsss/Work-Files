  <!-- T20200717.0031 - OWS TC - Display Summary column in Alerts table (IRI): Summary column added -->
  <SQLQuery><![CDATA[SELECT XwrlResponseIndColumns.LISTFULLNAME,              
       XwrlResponseIndColumns.LISTNAMETYPE,              
       XwrlResponseIndColumns.LISTFAMILYNAME,              
       XwrlResponseIndColumns.LISTGIVENNAMES,              
       XwrlResponseIndColumns.LISTDOB,              
       XwrlResponseIndColumns.DNYOB,              
       XwrlResponseIndColumns.DNGENDER,              
       XwrlResponseIndColumns.DECEASEDFLAG,              
       XwrlResponseIndColumns.DECEASEDDATE,              
       XwrlResponseIndColumns.DNRESIDENCYCOUNTRYCODE,              
       XwrlResponseIndColumns.LISTNATIONALITY,              
       XwrlResponseIndColumns.LISTCOUNTRY,              
       XwrlResponseIndColumns.LISTCITY,              
       XwrlResponseIndColumns.DNSTATE,              
       XwrlResponseIndColumns.LISTCOUNTRYOFBIRTH,              
       XwrlResponseIndColumns.DNADDRESS,              
       XwrlResponseIndColumns.DNTITLE,              
       XwrlResponseIndColumns.DNPASSPORTNUMBER,              
       XwrlResponseIndColumns.DNNATIONALID,              
       XwrlResponseIndColumns.CATEGORY,              
       XwrlResponseIndColumns.EXTERNALSOURCES,              
       XwrlResponseIndColumns.CACHEDEXTSOURCES,              
       XwrlResponseIndColumns.DNADDEDDATE,              
       XwrlResponseIndColumns.DNLASTUPDATEDDATE,              
       XwrlResponseIndColumns.MATCHSCORE,              
       XwrlResponseIndColumns.LISTID,              
       XwrlResponseIndColumns.LISTKEY,              
       XwrlResponseIndColumns.LISTSUBKEY,              
       XwrlResponseIndColumns.LISTRECORDTYPE,              
       XwrlResponseIndColumns.LISTPRIMARYNAME,              
       XwrlResponseIndColumns.LISTORIGINALSCRIPTNAME,              
       XwrlResponseIndColumns.MATCHRULE,              
       XwrlResponseIndColumns.CASEKEY,              
       XwrlResponseIndColumns.ALERTID,              
       XwrlResponseIndColumns.RISKSCORE,              
       XwrlResponseIndColumns.RISKSCOREPEP,              
       XwrlResponseIndColumns.DNOCCUPATION,              
       XwrlResponseIndColumns.DNCITY,              
       XwrlResponseIndColumns.DNPOSTALCODE,              
       XwrlResponseIndColumns.DNADDRESSCOUNTRYCODE,              
       XwrlResponseIndColumns.DNNATIONALITYCOUNTRYCODES,              
       XwrlResponseIndColumns.CREATED_BY,              
       XwrlResponseIndColumns.CREATION_DATE,              
       XwrlResponseIndColumns.DNCOUNTRYOFBIRTHCODE,              
       XwrlResponseIndColumns.ID,              
       XwrlResponseIndColumns.LAST_UPDATE_DATE,              
       XwrlResponseIndColumns.LAST_UPDATE_LOGIN,              
       XwrlResponseIndColumns.LAST_UPDATED_BY,              
       XwrlResponseIndColumns.LISTRECORDORIGIN,              
       XwrlResponseIndColumns.REC,              
       XwrlResponseIndColumns.REQUEST_ID,              
       XwrlResponseIndColumns.X_STATE,           
       XwrlResponseIndColumns.LEGAL_REVIEW,       
       nvl(XwrlResponseIndColumns.LEGAL_REVIEW,'N') LEGAL_REVIEW_ATTR,     
       XwrlResponseIndColumns.corporate_review  
       ,RMI_OWS_COMMON_UTIL.GET_OWS_ALERT_STATE(XwrlResponseIndColumns.ALERTID) owsAlertState        
       , decode(UTL_MATCH.edit_distance(XwrlResponseIndColumns.X_STATE,XWRL_OWS_UTILS.CHANGEOWSSTATE(RMI_OWS_COMMON_UTIL.GET_OWS_ALERT_STATE(XwrlResponseIndColumns.ALERTID))),0,'Y','N') as sameState      
       ,(select value from xwrl_response_rows where request_id=XwrlResponseIndColumns.request_id and XwrlResponseIndColumns.rec=rec_row and display='Y' and label='Summary') Summary,      
       substr(XwrlResponseIndColumns.x_state,7,length(XwrlResponseIndColumns.x_state)) stateAttr      
       ,DECODE(XwrlResponseIndColumns.LEGAL_REVIEW,'Y','Legal Checked','Legal Unchecked') LEGAL_CHECKED      
	   ,CASE      
  WHEN substr(XwrlResponseIndColumns.x_state,7,length(XwrlResponseIndColumns.x_state)) = 'Open'      
  THEN       
       substr(XwrlResponseIndColumns.x_state,7,length(XwrlResponseIndColumns.x_state))       
       ||' '||       
       DECODE(XwrlResponseIndColumns.LEGAL_REVIEW,'Y','Legal Checked','Legal Unchecked')      
   WHEN substr(XwrlResponseIndColumns.x_state,7,length(XwrlResponseIndColumns.x_state)) = 'False Positive'      
  THEN       
       substr(XwrlResponseIndColumns.x_state,7,length(XwrlResponseIndColumns.x_state))       
   WHEN substr(XwrlResponseIndColumns.x_state,7,length(XwrlResponseIndColumns.x_state)) = 'Possible'      
  THEN       
       substr(XwrlResponseIndColumns.x_state,7,length(XwrlResponseIndColumns.x_state))       
    WHEN substr(XwrlResponseIndColumns.x_state,7,length(XwrlResponseIndColumns.x_state)) = 'Positive'      
  THEN       
       substr(XwrlResponseIndColumns.x_state,7,length(XwrlResponseIndColumns.x_state))           
END as STATE_LEGAL      
FROM  XWRL_RESPONSE_IND_COLUMNS XwrlResponseIndColumns            
order by decode ((CASE      
  WHEN substr(XwrlResponseIndColumns.x_state,7,length(XwrlResponseIndColumns.x_state)) = 'Open'      
  THEN       
       substr(XwrlResponseIndColumns.x_state,7,length(XwrlResponseIndColumns.x_state))       
       ||' '||       
       DECODE(XwrlResponseIndColumns.LEGAL_REVIEW,'Y','Legal Checked','Legal Unchecked')      
   WHEN substr(XwrlResponseIndColumns.x_state,7,length(XwrlResponseIndColumns.x_state)) = 'False Positive'      
  THEN       
       substr(XwrlResponseIndColumns.x_state,7,length(XwrlResponseIndColumns.x_state))       
   WHEN substr(XwrlResponseIndColumns.x_state,7,length(XwrlResponseIndColumns.x_state)) = 'Possible'      
  THEN       
       substr(XwrlResponseIndColumns.x_state,7,length(XwrlResponseIndColumns.x_state))       
    WHEN substr(XwrlResponseIndColumns.x_state,7,length(XwrlResponseIndColumns.x_state)) = 'Positive'      
  THEN       
       substr(XwrlResponseIndColumns.x_state,7,length(XwrlResponseIndColumns.x_state))           
END)         
                 ,'Open Legal Unchecked',1        
                 ,'Open Legal Checked',2       
                 ,'Positive',4         
                 ,'Possible',3         
                 ,'False Positive',5) asc, XwrlResponseIndColumns.REC desc]]></SQLQuery>
