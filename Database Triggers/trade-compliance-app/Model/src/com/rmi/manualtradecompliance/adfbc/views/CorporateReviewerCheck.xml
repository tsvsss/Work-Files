<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="CorporateReviewerCheck"
  Version="12.1.3.10.47"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="com.rmi.manualtradecompliance.adfbc.views.CorporateReviewerCheck_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="pUserName"
    Kind="where"
    Type="java.lang.String"/>
  <SQLQuery><![CDATA[SELECT fu.user_id, fa.lookup_type, fa.lookup_code    
        FROM fnd_lookup_values  fa,      
             fnd_user           fu      
        WHERE fu.user_name = fa.lookup_code      
              AND lookup_type = 'CORPORATE_REVIEWERS'      
              and end_date_active is null      
              and fu.user_name=:pUserName]]></SQLQuery>
  <ViewAttribute
    Name="UserId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.lang.Long"
    ColumnType="LONGINT"
    AliasName="USER_ID"
    Expression="USER_ID"
    SQLType="NUMERIC"/>
  <ViewAttribute
    Name="LookupType"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="30"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="LOOKUP_TYPE"
    Expression="LOOKUP_TYPE"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="30"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="LookupCode"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="30"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="LOOKUP_CODE"
    Expression="LOOKUP_CODE"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="30"/>
    </DesignTime>
  </ViewAttribute>
  <AttrArray Name="KeyAttributes">
    <Item Value="UserId"/>
  </AttrArray>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="com.rmi.manualtradecompliance.adfbc.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
