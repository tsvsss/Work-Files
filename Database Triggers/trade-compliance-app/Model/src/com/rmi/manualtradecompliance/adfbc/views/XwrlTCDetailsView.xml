    XWRL_TC_DETAILS_VW.START_DATE START_DATE,          
    XWRL_TC_DETAILS_VW.END_DATE END_DATE,          
    XWRL_TC_DETAILS_VW.CONSENT_ID CONSENT_ID,          
    XWRL_TC_DETAILS_VW.EDOC_ID EDOC_ID,          
    XWRL_TC_DETAILS_VW.AFFIDAVIT_SINGLE_NAME AFFIDAVIT_SINGLE_NAME,          
    XWRL_TC_DETAILS_VW.KNOWN_PARTY KNOWN_PARTY,          
    XWRL_TC_DETAILS_VW.BATCH_ID BATCH_ID,          
    XWRL_TC_DETAILS_VW.REQUEST_ID REQUEST_ID,          
    XWRL_TC_DETAILS_VW.CONC_REQUEST_ID CONC_REQUEST_ID,          
    XWRL_TC_DETAILS_VW.CONC_PHASE_CODE CONC_PHASE_CODE,          
    XWRL_TC_DETAILS_VW.CONC_STATUS_CODE CONC_STATUS_CODE,          
    XWRL_TC_DETAILS_VW.CONC_REQUEST_STATUS CONC_REQUEST_STATUS,          
    XWRL_TC_DETAILS_VW.COUNTRY_OF_RESIDENCE_CODE COUNTRY_OF_RESIDENCE_CODE,          
    XWRL_TC_DETAILS_VW.COUNTRY_OF_RESIDENCE COUNTRY_OF_RESIDENCE,          
    XWRL_TC_DETAILS_VW.COUNTRY_OF_RESIDENCE_CITY COUNTRY_OF_RESIDENCE_CITY,          
