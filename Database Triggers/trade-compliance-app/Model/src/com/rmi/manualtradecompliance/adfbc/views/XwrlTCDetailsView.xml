    XWRL_TC_DETAILS_VW.RESIDENCE_CITY_ID RESIDENCE_CITY_ID,          
    XWRL_TC_DETAILS_VW.DEPARTMENT DEPARTMENT,          
    XWRL_TC_DETAILS_VW.DEPARTMENT_EXT DEPARTMENT_EXT,          
    XWRL_TC_DETAILS_VW.OFFICE OFFICE,          
    XWRL_TC_DETAILS_VW.DOCUMENT_TYPE DOCUMENT_TYPE,          
    XWRL_TC_DETAILS_VW.CASE_WORKFLOW CASE_WORKFLOW,          
    XWRL_TC_DETAILS_VW.CASE_STATE CASE_STATE,          
    XWRL_TC_DETAILS_VW.CASE_STATUS CASE_STATUS,          
    XWRL_TC_DETAILS_VW.EXPIRATION_DATE EXPIRATION_DATE,          
    XWRL_TC_DETAILS_VW.CASE_ID CASE_ID,          
    XWRL_TC_DETAILS_VW.VESSEL_INDICATOR VESSEL_INDICATOR,          
    XWRL_TC_DETAILS_VW.IMO_NUMBER IMO_NUMBER,          
    XWRL_TC_DETAILS_VW.CREATION_DATE CREATION_DATE,          
    XWRL_TC_DETAILS_VW.CREATED_USER CREATED_USER,          
    XWRL_TC_DETAILS_VW.LAST_UPDATE_DATE LAST_UPDATE_DATE,          
    (select description from fnd_user where user_name=XWRL_TC_DETAILS_VW.UPDATE_USER) UPDATE_USER,          
    XWRL_TC_DETAILS_VW.ASSIGNED_TO ASSIGNED_TO,        
    DECODE(XWRL_TC_DETAILS_VW.RELATIONSHIP_TYPE, 'Primary', 1, 'Alias', 3 , 2) RELATIONSHIP_ORDER,    
    XWRL_TC_DETAILS_VW.PRIMARY_NAME,    
XWRL_TC_DETAILS_VW.XREF_NAME,    
XWRL_TC_DETAILS_VW.SORT_XREF_ID,   
XWRL_TC_DETAILS_VW.INDENT_BY,  
XWRL_TC_DETAILS_VW.EAST_ASIAN_ORIGIN_NAME, 
XWRL_TC_DETAILS_VW.PRIMARY_MASTER_ID,
XWRL_TC_DETAILS_VW.PROVISIONAL_REQUEST
FROM          
    XWRL_TC_DETAILS_VW   
order by XWRL_TC_DETAILS_VW.primary_name,    
XWRL_TC_DETAILS_VW.xref_name nulls first,    
decode(XWRL_TC_DETAILS_VW.cursor,'c_1',1,'c_2',2,'c_3',3,4),    
full_name]]></SQLQuery>
