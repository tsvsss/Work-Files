     NVL ( ( Select user_name from RMI_SEAFARER.RMI_USER where user_id= WcRequestDocumentsEO.CREATED_BY),   
     (Select user_name from fnd_user where user_id= WcRequestDocumentsEO.CREATED_BY)) CreatedByName,     
