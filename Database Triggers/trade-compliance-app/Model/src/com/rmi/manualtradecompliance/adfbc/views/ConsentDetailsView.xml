    SQLType="DATE"/>
  <ViewAttribute
    Name="ConsentSource"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="50"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="CONSENT_SOURCE"
    Expression="CONSENT_SOURCE"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="50"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="EdocId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.lang.Integer"
    ColumnType="NUMBER"
    AliasName="EDOC_ID"
    Expression="EDOC_ID"
    SQLType="INTEGER"/>
  <ViewAttribute
    Name="EntityType"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
