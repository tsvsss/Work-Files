    <Item Value="ConsentId"/>
  </AttrArray>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="com.rmi.manualtradecompliance.adfbc.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
