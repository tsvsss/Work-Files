<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="ConsentDetailsView"
  Version="12.1.3.10.47"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="com.rmi.manualtradecompliance.adfbc.views.ConsentDetailsView_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="pSourceId"
    Kind="where"
    Type="java.lang.Integer"/>
  <Variable
    Name="pSourceTable"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="pMasterId"
    Kind="where"
    Type="java.lang.Integer"/>
  <Variable
    Name="pAliasId"
    Kind="where"
    Type="java.lang.Integer"/>
  <Variable
    Name="pXrefId"
    Kind="where"
    Type="java.lang.Integer"/>
  <SQLQuery><![CDATA[select consent_id,                
confirmation_number,                
consent_date,                
consent_expire_date,                
consent_source,                
edoc_id,    
entity_type, 
MARKETING_COMMUNICATION 
from RMI_DATA_PRIVACY_CONSENT                 
where consent_id =           
RMI_GENERIC_TC.get_consent_id(:pSourceTable,:pSourceId,:pMasterId,:pAliasId,:pXrefId)        
order by creation_date desc]]></SQLQuery>
  <ViewAttribute
    Name="ConsentId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.lang.Integer"
    ColumnType="NUMBER"
    AliasName="CONSENT_ID"
    Expression="CONSENT_ID"
    SQLType="INTEGER"/>
