  <SQLQuery><![CDATA[SELECT 
         full_name 
       , source_table_column 
       , m.source_table 
       , m.source_id 
       , m.ID 
       , TO_DATE (m.date_of_birth, 'YYYYMMDD') dob 
       , IDENTIFIER 
       , ENTITY_TYPE 
       , UPPER (SEX) SEX 
       , ID_TYPE 
       , m.PASSPORT_NUMBER 
       , PASSPORT_ISSUING_COUNTRY_CODE 
       ,(  
                SELECT  
                        country_code  
                FROM  
                        apps.sicd_countries  
                WHERE  
                        iso_alpha2_code = m.passport_issuing_country_code  
                AND     status          = 'Active'  
                AND     ROWNUM          = 1 ) PASSPORT_ISSUING_COUNTRY_CODE    
       , CITIZENSHIP_COUNTRY_CODE 
       ,(  
                SELECT  
                        country_code  
                FROM  
                        apps.sicd_countries  
                WHERE  
                        iso_alpha2_code = m.citizenship_country_code  
                AND     status          = 'Active'  
                AND     ROWNUM          = 1 ) CITIZENSHIP_COUNTRY_CODE    
       , m.COUNTRY_OF_RESIDENCE    
       , (    
                SELECT    
                       country_code    
                FROM    
                       apps.sicd_countries    
                WHERE    
                       iso_alpha2_code = m.COUNTRY_OF_RESIDENCE    
                       AND status      = 'Active'    
                       AND ROWNUM      = 1    
         )    
         COUNTRY_OF_RESIDENCE_CODE    
       , (    
                SELECT    
                       country_name    
                FROM    
                       apps.sicd_countries    
                WHERE    
                       iso_alpha2_code = m.COUNTRY_OF_RESIDENCE    
                       AND status      = 'Active'    
                       AND ROWNUM      = 1    
         )    
         COUNTRY_OF_RESIDENCE_NAME    
       , m.CITY_OF_RESIDENCE_ID    
       , (    
                SELECT    
                       CITY_NAME    
                       --T20220228.0033 - Enhance WC_CITY_LIST for Start Date/End Date    
                FROM    
                       WC_CITY_LIST_V    
                WHERE    
                       WC_CITY_LIST_ID = m.CITY_OF_RESIDENCE_ID    
                       --T20220302.0021 - Address Conflict is not listed under the city options for Ukraine or Russia    
                       --AND INTERNAL_USE_ONLY = 'N'    
         )    
                                         CITY_NAME    
       , NVL (LEGACY_VETTING_FLAG, 'No') LEGACY_VETTING_FLAG    
       , RELATIONSHIP_TYPE    
       , m.IMO_NUMBER    
       , m.EAST_ASIAN_ORIGIN_NAME    
FROM    
         xwrl_party_master m        
WHERE    
         m.id = :pId]]></SQLQuery>
