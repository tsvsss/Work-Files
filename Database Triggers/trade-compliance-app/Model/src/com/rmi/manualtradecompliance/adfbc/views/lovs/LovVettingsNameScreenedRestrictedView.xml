  <SQLQuery><![CDATA[SELECT full_name,   
         source_table_column,   
         m.source_table,   
         m.source_id,   
         m.ID,   
         TO_DATE (m.date_of_birth, 'YYYYMMDD') dob,   
         IDENTIFIER,   
         ENTITY_TYPE,   
         UPPER (SEX) SEX,   
         ID_TYPE,   
         m.PASSPORT_NUMBER,   
         PASSPORT_ISSUING_COUNTRY_CODE,   
         CITIZENSHIP_COUNTRY_CODE,   
         m.COUNTRY_OF_RESIDENCE,   
         (SELECT country_code   
            FROM apps.sicd_countries   
           WHERE     iso_alpha2_code = m.COUNTRY_OF_RESIDENCE   
                 AND status = 'Active'   
                 AND ROWNUM = 1)   
            COUNTRY_OF_RESIDENCE_CODE,   
         (SELECT country_name   
            FROM apps.sicd_countries   
           WHERE     iso_alpha2_code = m.COUNTRY_OF_RESIDENCE   
                 AND status = 'Active'   
                 AND ROWNUM = 1)   
            COUNTRY_OF_RESIDENCE_NAME,   
         m.CITY_OF_RESIDENCE_ID,   
         (SELECT CITY_NAME  
         --T20220228.0033 - Enhance WC_CITY_LIST for Start Date/End Date
            FROM WC_CITY_LIST_V   
           WHERE     WC_CITY_LIST_ID = m.CITY_OF_RESIDENCE_ID   
           --T20220302.0021 - Address Conflict is not listed under the city options for Ukraine or Russia
                 --AND INTERNAL_USE_ONLY = 'N'
                 )   
            CITY_NAME,   
         NVL (LEGACY_VETTING_FLAG, 'No') LEGACY_VETTING_FLAG,   
         RELATIONSHIP_TYPE,   
         m.IMO_NUMBER,   
         r.id TC_request_id,   
         r1.batch_id tc_batch,   
         TRUNC (r1.last_update_date) TC_BATCH_UPDATE_date   
    FROM xwrl_party_master m, (  SELECT MAX (id) id, actual_master_id   
              FROM xwrl_requests   
              WHERE alias_id IS NULL   
              AND NVL (case_state, 'Xx') NOT IN ('D')   
          GROUP BY actual_master_id) r,   
         xwrl_requests r1   
   WHERE TRIM (full_name) IS NOT NULL   
         AND end_date IS NULL   
         AND r.actual_master_id(+) = m.id   
         AND r1.id(+) = r.id  
         AND EXISTS ( 
        SELECT 
            1 
        FROM 
            xwrl_party_master m1 
        WHERE 
            m1.relationship_type = 'Primary' 
            AND m1.source_table = :p_source_table 
            AND m1.source_id = :p_source_id 
            AND m1.end_date IS NULL 
            AND m1.id = m.id 
        UNION ALL 
        SELECT 
            1 
        FROM 
            xwrl_party_master m2, 
            xwrl_party_xref   x1 
        WHERE 
            x1.master_id = m2.id 
            AND m2.source_table = :p_source_table 
            AND m2.source_id = :p_source_id 
            AND x1.end_date IS NULL 
            AND x1.relationship_master_id = m.id 
            AND x1.relationship_master_id != x1.master_id 
    ) 
ORDER BY 1]]></SQLQuery>
