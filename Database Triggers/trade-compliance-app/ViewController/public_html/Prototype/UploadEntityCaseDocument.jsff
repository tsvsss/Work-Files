<?xml version='1.0' encoding='UTF-8'?>
<ui:composition xmlns:ui="http://java.sun.com/jsf/facelets" xmlns:af="http://xmlns.oracle.com/adf/faces/rich"
                xmlns:f="http://java.sun.com/jsf/core" xmlns:h="http://java.sun.com/jsf/html">
  <af:panelHeader text="Entity Case" id="ph3" >
    <f:facet name="context">
      <af:outputText value="#{bindings.CaseId.inputValue}" shortDesc="#{bindings.CaseId.hints.tooltip}" id="ot1"/>
    </f:facet>
    <f:facet name="menuBar"/>
    <f:facet name="toolbar"/>
    <f:facet name="legend"/>
    <f:facet name="info"/>
    <af:panelTabbed position="above" id="pt1">
      <af:showDetailItem id="tab2" text="Data Privacy Consent">
        <af:region value="#{bindings.DataPrivacyConsentTaskFlow1.regionModel}" id="r1"/>
      </af:showDetailItem>
      <af:showDetailItem id="tab1" text="Case Documents" disclosed="true">
        <af:panelSplitter id="ps1" splitterPosition="300" styleClass="CustomizedBorderSplitter">
          <f:facet name="first">
            <af:query id="qryId2" headerText="Search" disclosed="true" saveQueryMode="hidden" modeChangeVisible="false"
                      value="#{bindings.XwrlCaseDocumentsSumViewCriteriaQuery.queryDescriptor}"
                      model="#{bindings.XwrlCaseDocumentsSumViewCriteriaQuery.queryModel}"
                      queryListener="#{bindings.XwrlCaseDocumentsSumViewCriteriaQuery.processQuery}"
                      queryOperationListener="#{bindings.XwrlCaseDocumentsSumViewCriteriaQuery.processQueryOperation}"
                      resultComponentId="::pc1:t3" styleClass="CustomQueryPanel"/>
          </f:facet>
          <f:facet name="second">
            <af:panelHeader text="Case Documents" id="ph2">
              <f:facet name="context">
                <af:toolbar id="t2"/>
              </f:facet>
              <f:facet name="menuBar"/>
              <f:facet name="toolbar">
                <af:button text="Return" id="b3" action="goEntityRequest" rendered="false"/>
              </f:facet>
              <f:facet name="legend"/>
              <f:facet name="info"/>
              <af:popup childCreation="deferred" autoCancel="disabled" id="p3" contentDelivery="lazyUncached">
