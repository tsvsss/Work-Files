                      <af:convertNumber groupingUsed="false"
                                        pattern="#{bindings.XwrlCaseDocumentsSumView1.hints.CaseDocId.format}"/>
                    </af:outputText>
                  </af:column>
                  <af:column headerText="#{bindings.XwrlCaseDocumentsSumView1.hints.DocumentType.label}" id="c8"
