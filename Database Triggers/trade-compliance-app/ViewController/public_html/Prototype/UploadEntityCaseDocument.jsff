                                      columns="#{bindings.FilePath.hints.displayWidth}"
                                      maximumLength="#{bindings.FilePath.hints.precision}"
                                      shortDesc="#{bindings.FilePath.hints.tooltip}" id="it9" autoSubmit="true">
                          <f:validator binding="#{bindings.FilePath.validator}"/>
                        </af:inputText>
                        <af:inputText value="#{bindings.ContentType.inputValue}"
