                           dialogListener="#{viewScope.CaseDocumentsBean.deleteDocumentDialogListener}">
                  <af:panelGridLayout id="pgl1" dimensionsFrom="auto">
                    <af:gridRow marginTop="5px" marginBottom="5px" id="gr1">
