                      <f:attribute name="Id" value="#{row.CaseDocId}"/>
                      <af:fileDownloadActionListener method="#{viewScope.CaseDocumentsBean.downloadCaseDocsFileListener}"
                                                     filename="#{row.FileName}"/>
                    </af:link>
                  </af:column>
                </af:table>
