                    </af:gridRow>
                  </af:panelGridLayout>
                  <f:facet name="buttonBar"/>
                </af:dialog>
              </af:popup>
              <af:panelCollection id="pc1" styleClass="AFStretchWidth">
                <f:facet name="menus"/>
                <f:facet name="toolbar">
                  <af:toolbar id="t1">
                    <af:button text="Upload" id="b1" useWindow="true" windowHeight="700" windowWidth="1000"
                               windowEmbedStyle="inlineDocument" windowModalityType="applicationModal"
                               action="#{pageFlowScope.UploadModel.uploadActionListener}"
                               returnListener="#{pageFlowScope.UploadModel.uploadReturnListener}">
                      <af:setPropertyListener type="action" from="#{'Entity Case'}" to="#{pageFlowScope.panelHeader}"/>
                      <af:setPropertyListener type="action" from="#{bindings.CaseId1.inputValue}"
                                              to="#{pageFlowScope.displayKey}"/>
                      <af:setPropertyListener type="action" from="#{bindings.XwrlCaseDocumentsView2Iterator.name}"
                                              to="#{pageFlowScope.iterBinding}"/>
                      <af:setPropertyListener type="action" from="#{'aot2'}" to="#{pageFlowScope.outputTextId}"/>
                    </af:button>
                    <af:button text="Edit" id="b2" actionListener="#{viewScope.CaseDocumentsBean.docShowPopup}"
                               disabled="#{!bindings.Delete.enabled}" rendered="false"/>
                    <af:button text="Delete" id="b4" actionListener="#{viewScope.CaseDocumentsBean.deleteShowPopup}" partialTriggers="t3">
                      <af:setPropertyListener type="action" to="#{pageFlowScope.iterBinding}"
                                              from="#{'XwrlCaseDocumentsView2Iterator'}"/>
                      <af:setPropertyListener type="action" from="#{'XwrlRequestsView1Iterator'}"
                                              to="#{pageFlowScope.masterIter}"/>
                    </af:button>
                    <af:button actionListener="#{bindings.Execute1.execute}" text="Refresh"
                               disabled="#{!bindings.Execute1.enabled}" id="b5" partialTriggers="t3"/>
                  </af:toolbar>
