          <f:facet name="second">
            <af:panelBox text="Detailed Message" id="pb1">
              <af:inputText label="Label 2" id="it5" simple="true" value="#{bindings.DetailedMessage.inputValue}"
                            partialTriggers="qryId2 pc2:t3" readOnly="true" rows="20" styleClass="AFStretchWidth"
                            contentStyle="width: 100%; resize: none;" dimensionsFrom="parent"/>
            </af:panelBox>
