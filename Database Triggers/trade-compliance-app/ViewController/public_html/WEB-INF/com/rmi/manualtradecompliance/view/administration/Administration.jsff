                    </af:column>
                    <af:column sortProperty="#{bindings.RmiGenericLogsView1.hints.Param2.name}" sortable="true"
                               headerText="#{bindings.RmiGenericLogsView1.hints.Param2.label}" id="c18">
                      <af:outputText value="#{row.Param2}"
                                     shortDesc="#{bindings.RmiGenericLogsView1.hints.Param2.tooltip}" id="ot14"/>
                    </af:column>
                    <af:column sortProperty="#{bindings.RmiGenericLogsView1.hints.Param3.name}" sortable="true"
                               headerText="#{bindings.RmiGenericLogsView1.hints.Param3.label}" id="c19">
                      <af:outputText value="#{row.Param3}"
                                     shortDesc="#{bindings.RmiGenericLogsView1.hints.Param3.tooltip}" id="ot15"/>
                    </af:column>
                    <af:column sortProperty="#{bindings.RmiGenericLogsView1.hints.Param4.name}" sortable="true"
                               headerText="#{bindings.RmiGenericLogsView1.hints.Param4.label}" id="c20">
                      <af:outputText value="#{row.Param4}"
                                     shortDesc="#{bindings.RmiGenericLogsView1.hints.Param4.tooltip}" id="ot16"/>
                    </af:column>
                    <af:column sortProperty="#{bindings.RmiGenericLogsView1.hints.Param5.name}" sortable="true"
                               headerText="#{bindings.RmiGenericLogsView1.hints.Param5.label}" id="c21">
                      <af:outputText value="#{row.Param5}"
                                     shortDesc="#{bindings.RmiGenericLogsView1.hints.Param5.tooltip}" id="ot17"/>
                    </af:column>
                    <af:column sortProperty="#{bindings.RmiGenericLogsView1.hints.Attribute1.name}" sortable="true"
                               headerText="#{bindings.RmiGenericLogsView1.hints.Attribute1.label}" id="c22">
                      <af:outputText value="#{row.Attribute1}"
                                     shortDesc="#{bindings.RmiGenericLogsView1.hints.Attribute1.tooltip}" id="ot18"/>
                    </af:column>
                    <af:column sortProperty="#{bindings.RmiGenericLogsView1.hints.Attribute2.name}" sortable="true"
                               headerText="#{bindings.RmiGenericLogsView1.hints.Attribute2.label}" id="c23">
                      <af:outputText value="#{row.Attribute2}"
                                     shortDesc="#{bindings.RmiGenericLogsView1.hints.Attribute2.tooltip}" id="ot19"/>
                    </af:column>
                    <af:column sortProperty="#{bindings.RmiGenericLogsView1.hints.Attribute3.name}" sortable="true"
                               headerText="#{bindings.RmiGenericLogsView1.hints.Attribute3.label}" id="c24">
                      <af:outputText value="#{row.Attribute3}"
                                     shortDesc="#{bindings.RmiGenericLogsView1.hints.Attribute3.tooltip}" id="ot20"/>
                    </af:column>
                    <af:column sortProperty="#{bindings.RmiGenericLogsView1.hints.Attribute4.name}" sortable="true"
