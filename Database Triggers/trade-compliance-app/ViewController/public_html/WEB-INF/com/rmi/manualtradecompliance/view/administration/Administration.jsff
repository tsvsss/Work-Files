                    </af:column>
                    <af:column sortProperty="#{bindings.RmiGenericLogsView1.hints.Param1.name}" sortable="true"
                               headerText="#{bindings.RmiGenericLogsView1.hints.Param1.label}" id="c17">
                      <af:outputText value="#{row.Param1}"
