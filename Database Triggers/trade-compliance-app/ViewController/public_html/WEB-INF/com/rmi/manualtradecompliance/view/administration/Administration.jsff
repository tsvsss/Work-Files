                            binding="#{viewScope.AdministrationBean.logsTableObj}" partialTriggers="::b2 ::b5 ::b6">
                    <af:column headerText="#{bindings.RmiGenericLogsView1.hints.LogId.label}" id="c5" width="50"
                               sortable="true" sortProperty="#{bindings.RmiGenericLogsView1.hints.LogId.name}">
                      <af:outputText value="#{row.LogId}"
                                     shortDesc="#{bindings.RmiGenericLogsView1.hints.LogId.tooltip}" id="ot1">
                        <af:convertNumber groupingUsed="false"
                                          pattern="#{bindings.RmiGenericLogsView1.hints.LogId.format}"/>
                      </af:outputText>
                    </af:column>
                    <af:column sortProperty="#{bindings.RmiGenericLogsView1.hints.SourceId.name}" sortable="true"
                               headerText="#{bindings.RmiGenericLogsView1.hints.SourceId.label}" id="c13" width="60">
                      <af:outputText value="#{row.SourceId}"
                                     shortDesc="#{bindings.RmiGenericLogsView1.hints.SourceId.tooltip}" id="ot9">
                        <af:convertNumber groupingUsed="false"
                                          pattern="#{bindings.RmiGenericLogsView1.hints.SourceId.format}"/>
                      </af:outputText>
                    </af:column>
                    <af:column sortProperty="#{bindings.RmiGenericLogsView1.hints.CreationDate.name}" sortable="true"
                               headerText="#{bindings.RmiGenericLogsView1.hints.CreationDate.label}" id="c12"
                               width="80">
                      <af:outputText value="#{row.CreationDate}"
                                     shortDesc="#{bindings.RmiGenericLogsView1.hints.CreationDate.tooltip}" id="ot8">
                        <af:convertDateTime pattern="#{bindings.RmiGenericLogsView1.hints.CreationDate.format}"/>
                      </af:outputText>
                    </af:column>
                    <af:column headerText="#{bindings.RmiGenericLogsView1.hints.CreatedUser.label}" id="c11"
                               width="130">
                      <af:outputText value="#{row.CreatedUser}"
                                     shortDesc="#{bindings.RmiGenericLogsView1.hints.CreatedUser.tooltip}" id="ot7"/>
                    </af:column>
                    <af:column sortProperty="#{bindings.RmiGenericLogsView1.hints.Title.name}" sortable="true"
                               headerText="#{bindings.RmiGenericLogsView1.hints.Title.label}" id="c7" width="200">
                      <af:outputText value="#{row.Title}"
                                     shortDesc="#{bindings.RmiGenericLogsView1.hints.Title.tooltip}" id="ot3"/>
                    </af:column>
                    <af:column sortProperty="#{bindings.RmiGenericLogsView1.hints.ClassName.name}" sortable="true"
                               headerText="#{bindings.RmiGenericLogsView1.hints.ClassName.label}" id="c9">
                      <af:outputText value="#{row.ClassName}"
                                     shortDesc="#{bindings.RmiGenericLogsView1.hints.ClassName.tooltip}" id="ot5"/>
                    </af:column>
                    <af:column sortProperty="#{bindings.RmiGenericLogsView1.hints.MethodName.name}" sortable="true"
                               headerText="#{bindings.RmiGenericLogsView1.hints.MethodName.label}" id="c10">
                      <af:outputText value="#{row.MethodName}"
                                     shortDesc="#{bindings.RmiGenericLogsView1.hints.MethodName.tooltip}" id="ot6"/>
                    </af:column>
                    <af:column sortProperty="#{bindings.RmiGenericLogsView1.hints.SourceTable.name}" sortable="true"
                               headerText="#{bindings.RmiGenericLogsView1.hints.SourceTable.label}" id="c14"
                               width="110">
                      <af:outputText value="#{row.SourceTable}"
                                     shortDesc="#{bindings.RmiGenericLogsView1.hints.SourceTable.tooltip}" id="ot10"/>
                    </af:column>
                    <af:column sortProperty="#{bindings.RmiGenericLogsView1.hints.SourceTableColumn.name}"
                               sortable="true"
                               headerText="#{bindings.RmiGenericLogsView1.hints.SourceTableColumn.label}" id="c15">
                      <af:outputText value="#{row.SourceTableColumn}"
                                     shortDesc="#{bindings.RmiGenericLogsView1.hints.SourceTableColumn.tooltip}"
                                     id="ot11"/>
                    </af:column>
                    <af:column sortProperty="#{bindings.RmiGenericLogsView1.hints.SourceType.name}" sortable="true"
                               headerText="#{bindings.RmiGenericLogsView1.hints.SourceType.label}" id="c16">
                      <af:outputText value="#{row.SourceType}"
                                     shortDesc="#{bindings.RmiGenericLogsView1.hints.SourceType.tooltip}" id="ot12"/>
                    </af:column>
                    <af:column sortProperty="#{bindings.RmiGenericLogsView1.hints.Type.name}" sortable="true"
                               headerText="#{bindings.RmiGenericLogsView1.hints.Type.label}" id="c6" width="50">
                      <af:outputText value="#{row.Type}" shortDesc="#{bindings.RmiGenericLogsView1.hints.Type.tooltip}"
                                     id="ot2"/>
                    </af:column>
                    <af:column sortProperty="#{bindings.RmiGenericLogsView1.hints.Message.name}" sortable="true"
                               headerText="#{bindings.RmiGenericLogsView1.hints.Message.label}" id="c8">
                      <af:outputText value="#{row.Message}"
