<?xml version='1.0' encoding='UTF-8'?>
<jsp:root xmlns:jsp="http://java.sun.com/JSP/Page" version="2.1" xmlns:af="http://xmlns.oracle.com/adf/faces/rich"
          xmlns:f="http://java.sun.com/jsf/core">
  <af:panelTabbed position="above" id="pt1" >
    <af:showDetailItem id="parametersTab" text="Parameters" disclosed="true">
      <af:panelSplitter id="ps1" splitterPosition="300">
        <f:facet name="first">
          <af:panelGroupLayout layout="vertical" id="pgl1">
            <af:query id="qryId1" headerText="Search" disclosed="true"
                      value="#{bindings.RmiGenericParametersViewCriteriaQuery.queryDescriptor}"
                      model="#{bindings.RmiGenericParametersViewCriteriaQuery.queryModel}"
                      queryListener="#{bindings.RmiGenericParametersViewCriteriaQuery.processQuery}"
                      queryOperationListener="#{bindings.RmiGenericParametersViewCriteriaQuery.processQueryOperation}"
                      saveQueryMode="hidden"/>
          </af:panelGroupLayout>
        </f:facet>
        <f:facet name="second">
          <af:panelCollection id="pc1" styleClass="AFStretchWidth">
            <f:facet name="toolbar">
              <af:toolbar id="t2">
                <af:button text="Add" id="b1" actionListener="#{bindings.CreateInsert.execute}"/>
                <af:spacer width="10" height="10" id="s1"/>
                <af:button text="Save" id="b3" actionListener="#{bindings.Commit.execute}"/>
                <af:button text="Cancel" id="b4" actionListener="#{bindings.Rollback.execute}" immediate="true">
                  <af:resetActionListener/>
                </af:button>
              </af:toolbar>
            </f:facet>
            <af:table value="#{bindings.RmiGenericParametersView1.collectionModel}" var="row"
                      rows="#{bindings.RmiGenericParametersView1.rangeSize}"
                      emptyText="#{bindings.RmiGenericParametersView1.viewable ? 'No data to display.' : 'Access Denied.'}"
                      rowBandingInterval="0"
                      selectedRowKeys="#{bindings.RmiGenericParametersView1.collectionModel.selectedRow}"
                      selectionListener="#{bindings.RmiGenericParametersView1.collectionModel.makeCurrent}"
                      rowSelection="single" fetchSize="#{bindings.RmiGenericParametersView1.rangeSize}" id="t1"
                      styleClass="AFStretchWidth" columnStretching="column:c4" partialTriggers="::b1 ::b3 ::b4"
                      editingMode="clickToEdit">
              <af:column sortProperty="#{bindings.RmiGenericParametersView1.hints.Id.name}" sortable="true"
                         headerText="#{bindings.RmiGenericParametersView1.hints.Id.label}" id="c1" width="200">
                <af:inputText value="#{row.bindings.Id.inputValue}"
                              label="#{bindings.RmiGenericParametersView1.hints.Id.label}"
                              required="#{bindings.RmiGenericParametersView1.hints.Id.mandatory}"
                              maximumLength="#{bindings.RmiGenericParametersView1.hints.Id.precision}"
                              shortDesc="#{bindings.RmiGenericParametersView1.hints.Id.tooltip}" id="it1">
                  <f:validator binding="#{row.bindings.Id.validator}"/>
                </af:inputText>
              </af:column>
              <af:column sortProperty="#{bindings.RmiGenericParametersView1.hints.OptionKey.name}" sortable="true"
                         headerText="#{bindings.RmiGenericParametersView1.hints.OptionKey.label}" id="c2" width="200">
                <af:inputText value="#{row.bindings.OptionKey.inputValue}"
                              label="#{bindings.RmiGenericParametersView1.hints.OptionKey.label}"
                              required="#{bindings.RmiGenericParametersView1.hints.OptionKey.mandatory}"
                              maximumLength="#{bindings.RmiGenericParametersView1.hints.OptionKey.precision}"
                              shortDesc="#{bindings.RmiGenericParametersView1.hints.OptionKey.tooltip}" id="it2">
                  <f:validator binding="#{row.bindings.OptionKey.validator}"/>
                </af:inputText>
              </af:column>
              <af:column sortProperty="#{bindings.RmiGenericParametersView1.hints.OptionValue.name}" sortable="true"
                         headerText="#{bindings.RmiGenericParametersView1.hints.OptionValue.label}" id="c3" width="200">
                <af:inputText value="#{row.bindings.OptionValue.inputValue}"
                              label="#{bindings.RmiGenericParametersView1.hints.OptionValue.label}"
                              required="#{bindings.RmiGenericParametersView1.hints.OptionValue.mandatory}"
                              maximumLength="#{bindings.RmiGenericParametersView1.hints.OptionValue.precision}"
                              shortDesc="#{bindings.RmiGenericParametersView1.hints.OptionValue.tooltip}" id="it3">
                  <f:validator binding="#{row.bindings.OptionValue.validator}"/>
                </af:inputText>
              </af:column>
              <af:column headerText="#{bindings.RmiGenericParametersView1.hints.Enabled.label}" id="c27" width="60">
                <af:selectBooleanCheckbox value="#{row.bindings.Enabled.inputValue}"
                                          label="#{row.bindings.Enabled.label}"
                                          shortDesc="#{bindings.RmiGenericParametersView1.hints.Enabled.tooltip}"
                                          id="sbc1"/>
              </af:column>
              <af:column sortProperty="#{bindings.RmiGenericParametersView1.hints.Description.name}" sortable="true"
                         headerText="#{bindings.RmiGenericParametersView1.hints.Description.label}" id="c4">
                <af:inputText value="#{row.bindings.Description.inputValue}"
                              label="#{bindings.RmiGenericParametersView1.hints.Description.label}"
                              required="#{bindings.RmiGenericParametersView1.hints.Description.mandatory}"
                              maximumLength="#{bindings.RmiGenericParametersView1.hints.Description.precision}"
                              shortDesc="#{bindings.RmiGenericParametersView1.hints.Description.tooltip}" id="it4">
                  <f:validator binding="#{row.bindings.Description.validator}"/>
                </af:inputText>
              </af:column>
            </af:table>
          </af:panelCollection>
        </f:facet>
      </af:panelSplitter>
    </af:showDetailItem>
