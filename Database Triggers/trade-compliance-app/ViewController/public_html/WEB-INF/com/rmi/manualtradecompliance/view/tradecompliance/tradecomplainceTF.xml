      </outcome>
    </method-call>
    <control-flow-rule id="__4">
      <from-activity-id>tradecomplaince</from-activity-id>
      <control-flow-case id="__5">
        <from-outcome>Close</from-outcome>
        <to-activity-id>taskFlowReturn1</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
  </task-flow-definition>
</adfc-config>
