<?xml version="1.0" encoding="UTF-8" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="task-flow-definition">
    <default-activity>tradecomplaince</default-activity>
    <data-control-scope>
      <isolated/>
    </data-control-scope>
    <input-parameter-definition id="__6">
      <name>P_SOURCE_ID</name>
      <value>#{pageFlowScope.P_SOURCE_ID}</value>
      <class>java.lang.Integer</class>
    </input-parameter-definition>
    <input-parameter-definition id="__7">
      <name>P_ESI_BATCH_ID_TC</name>
      <value>#{pageFlowScope.P_ESI_BATCH_ID_TC}</value>
      <class>java.lang.Integer</class>
    </input-parameter-definition>
    <managed-bean id="__1">
      <managed-bean-name>TradeComplainceBean</managed-bean-name>
      <managed-bean-class>com.rmi.manualtradecompliance.view.tradecompliance.TradeComplaince</managed-bean-class>
      <managed-bean-scope>backingBean</managed-bean-scope>
    </managed-bean>
    <managed-bean id="__3">
      <managed-bean-name>TCAssociatedDocBean</managed-bean-name>
      <managed-bean-class>com.rmi.manualtradecompliance.view.tradecompliance.TCAssociatedDoc</managed-bean-class>
      <managed-bean-scope>pageFlow</managed-bean-scope>
    </managed-bean>
    <exception-handler>exceptionHandler</exception-handler>
    <view id="tradecomplaince">
      <page>/WEB-INF/com/rmi/manualtradecompliance/view/tradecompliance/tradecomplaince.jsf</page>
    </view>
    <task-flow-return id="taskFlowReturn1">
