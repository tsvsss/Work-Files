<?xml version="1.0" encoding="UTF-8" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
<!-- T20201012.0026 - Linking E-Docs to Case Documents in OWS Statusboar... (IRI) -->
  <task-flow-definition id="IriEdocsTaskFlow">
    <default-activity>IRIEdocs</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <managed-bean id="__1">
      <managed-bean-name>CaseDocumentsBean</managed-bean-name>
      <managed-bean-class>oracle.apps.xwrl.beans.backing.CaseDocuments</managed-bean-class>
      <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <view id="IRIEdocs">
      <page>/Prototype/IRIEdocs.jsff</page>
    </view>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
