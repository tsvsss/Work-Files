<?xml version="1.0" encoding="UTF-8" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="CitylistTaskFlow">
    <default-activity>execCity</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <managed-bean id="__1">
      <managed-bean-name>Citylist</managed-bean-name>
      <managed-bean-class>oracle.apps.xwrl.beans.backing.Citylist</managed-bean-class>
      <managed-bean-scope>backingBean</managed-bean-scope>
    </managed-bean>
    <view id="Citylist">
      <page>/Prototype/Citylist.jsff</page>
    </view>
    <method-call id="execCity">
      <method>#{bindings.execCity.execute}</method>
      <outcome>
        <fixed-outcome>execCity</fixed-outcome>
      </outcome>
    </method-call>
    <control-flow-rule id="__2">
      <from-activity-id>execCity</from-activity-id>
      <control-flow-case id="__3">
        <from-outcome>execCity</from-outcome>
        <to-activity-id>Citylist</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
