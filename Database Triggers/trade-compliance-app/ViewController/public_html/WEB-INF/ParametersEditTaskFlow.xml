      </outcome>
    </task-flow-return>
    <control-flow-rule id="__1">
      <from-activity-id>RouteFlow</from-activity-id>
      <control-flow-case id="__3">
        <from-outcome>Edit</from-outcome>
        <to-activity-id>SetCurrentRowWithKey</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__4">
        <from-outcome>Add</from-outcome>
        <to-activity-id>UpdateParameters</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__10">
      <from-activity-id>UpdateParameters</from-activity-id>
      <control-flow-case id="__2">
        <from-outcome>Close</from-outcome>
        <to-activity-id>taskFlowReturn1</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__14">
      <from-activity-id>SetCurrentRowWithKey</from-activity-id>
      <control-flow-case id="__15">
        <from-outcome>setCurrentRowWithKey</from-outcome>
        <to-activity-id>UpdateParameters</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
  </task-flow-definition>
</adfc-config>
