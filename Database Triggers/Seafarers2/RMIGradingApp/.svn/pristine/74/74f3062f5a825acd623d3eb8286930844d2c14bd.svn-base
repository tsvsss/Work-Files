  VERIFICATION_METHOD,        
  VERIFICATION_URL,        
  VERIFICATION_EMAIL,                         
  VERIFICATION_USERNAME,       
  BIRTH_DATE,        
  VERIFICATION_PASSWORD       
FROM RMI_SICD_DOCUMENTS_WAITING_V          
WHERE NVL(verified,'P') = 'W'         
AND document_type ='OC'         
AND certificate_type ='OC'         
AND verification_request_date IS NOT NULL       
AND EXSICD_PROFILES_PKG.get_user_profile(created_by,'Default Processing Office')          
      =  (CASE WHEN :pUserId IS NOT NULL       
          THEN EXSICD_PROFILES_PKG.get_user_profile(:pUserId,'Default Processing Office')       
          ELSE EXSICD_PROFILES_PKG.get_user_profile(created_by,'Default Processing Office')       
          END)     
AND seafarer_id = :pSeafarerId]]></SQLQuery>
