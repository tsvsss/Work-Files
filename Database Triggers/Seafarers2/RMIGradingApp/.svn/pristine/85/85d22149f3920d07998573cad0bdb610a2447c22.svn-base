<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="AllPreviousDocsView"
  Version="12.1.3.10.8"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="com.rmi.tradecompapproval.adfbc.views.readonly.AllPreviousDocsView_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="pDocumentId"
    Kind="where"
    Type="java.lang.Integer">
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYHINT
          Value="Hide"/>
      </SchemaBasedProperties>
    </Properties>
  </Variable>
  <SQLQuery><![CDATA[SELECT DOCUMENT_TYPE,    
DECODE(DOCUMENT_TYPE, 'OC', CERTIFICATE_NUMBER,    
'BOOK', BOOK_NUMBER,    
'SQC', SQC_NUMBER) DOCUMENT_NUMBER,    
DOCUMENT_ID,    
ENDORSEMENT_NUMBER,    
TO_CHAR(EXPIRATION_DATE,'dd-MON-yyyy') EXPIRATION_DATE,    
STATUS,    
GRADE_ID    
FROM SICD.SICD_DOCUMENTS    
WHERE DOCUMENT_ID = :pDocumentId]]></SQLQuery>
  <ViewAttribute
    Name="DocumentType"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="255"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="DOCUMENT_TYPE"
    Expression="DOCUMENT_TYPE"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="10"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="DocumentNumber"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.lang.Integer"
    ColumnType="NUMBER"
    AliasName="DOCUMENT_NUMBER"
    Expression="DOCUMENT_NUMBER"
    SQLType="INTEGER"/>
  <ViewAttribute
    Name="DocumentId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="38"
    Scale="0"
    Type="java.lang.Integer"
    ColumnType="NUMBER"
    AliasName="DOCUMENT_ID"
    Expression="DOCUMENT_ID"
    SQLType="INTEGER"/>
  <ViewAttribute
    Name="EndorsementNumber"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="60"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="ENDORSEMENT_NUMBER"
    Expression="ENDORSEMENT_NUMBER"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="60"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="ExpirationDate"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="11"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="EXPIRATION_DATE"
    Expression="EXPIRATION_DATE"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="11"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="Status"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="10"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="STATUS"
    Expression="STATUS"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="10"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="GradeId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="38"
    Scale="0"
    Type="java.lang.Integer"
    ColumnType="NUMBER"
    AliasName="GRADE_ID"
    Expression="GRADE_ID"
    SQLType="INTEGER"/>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="com.rmi.tradecompapproval.adfbc.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
