<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="PreviousSicdOCView"
  Version="12.1.3.10.47"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="com.rmi.tradecompapproval.adfbc.views.lovs.PreviousSicdOCView_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="pSeafarerId"
    Kind="where"
    Type="java.lang.Integer">
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYHINT
          Value="Hide"/>
      </SchemaBasedProperties>
    </Properties>
  </Variable>
  <Variable
    Name="pDocumentId"
    Kind="where"
    Type="java.lang.Integer">
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYHINT
          Value="Hide"/>
      </SchemaBasedProperties>
    </Properties>
  </Variable>
  <SQLQuery><![CDATA[SELECT DCMT.CERTIFICATE_NUMBER CERTIFICATE_NUMBER,    
       DCMT.DOCUMENT_ID DOCUMENT_ID,    
       L_GRDE.GRADE_ID L_GRDE_GRADE_ID,    
       L_GRDE.GRADE_CODE L_GRDE_GRADE_CODE    
FROM SICD_DOCUMENTS DCMT,    
     SICD_GRADES L_GRDE    
WHERE DCMT.GRADE_ID = L_GRDE.GRADE_ID AND    
      /* CG$LOVI_WC_START LOV_OC 280 */    
      (dcmt.document_type = 'OC'     
        and dcmt.seafarer_id = :pSeafarerId     
        and dcmt.document_id <> NVL(:pDocumentId,0))    
      /* CG$LOVI_WC_END LOV_OC 280 */    
ORDER BY CERTIFICATE_NUMBER]]></SQLQuery>
  <ViewAttribute
    Name="CertificateNumber"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.lang.Integer"
    ColumnType="BIGINT"
    AliasName="CERTIFICATE_NUMBER"
    Expression="CERTIFICATE_NUMBER"
    SQLType="INTEGER">
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYWIDTH
          Value="30"/>
        <DISPLAYHEIGHT
          Value="10"/>
        <LABEL
          ResId="com.rmi.tradecompapproval.adfbc.views.lovs.PreviousSicdOCView.CertificateNumber_LABEL"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <ViewAttribute
    Name="DocumentId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="38"
    Scale="0"
    Type="java.math.BigDecimal"
    ColumnType="NUMBER"
    AliasName="DOCUMENT_ID"
    Expression="DOCUMENT_ID"
    SQLType="NUMERIC">
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYHINT
          Value="Hide"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <ViewAttribute
    Name="LGrdeGradeId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="38"
    Scale="0"
    Type="java.math.BigInteger"
    ColumnType="NUMBER"
    AliasName="L_GRDE_GRADE_ID"
    Expression="L_GRDE_GRADE_ID"
    SQLType="NUMERIC">
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYHINT
          Value="Hide"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <ViewAttribute
    Name="LGrdeGradeCode"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="10"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="L_GRDE_GRADE_CODE"
    Expression="L_GRDE_GRADE_CODE"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="10"/>
    </DesignTime>
    <Properties>
      <SchemaBasedProperties>
        <LABEL
          ResId="com.rmi.tradecompapproval.adfbc.views.lovs.PreviousSicdOCView.LGrdeGradeCode_LABEL"/>
        <DISPLAYWIDTH
          Value="15"/>
        <DISPLAYHEIGHT
          Value="15"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <AttrArray Name="KeyAttributes">
    <Item Value="DocumentId"/>
  </AttrArray>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="com.rmi.tradecompapproval.adfbc.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
