  <SQLQuery><![CDATA[SELECT L_GRDE.GRADE_CODE L_GRDE_GRADE_CODE,  
       PSTN.POSITION_ID POSITION_ID,  
       L_CPCT.CAPACITY_SEQUENCE L_CPCT_CAPACITY_SEQUENCE,  
       L_CPCT.CAPACITY_NAME L_CPCT_CAPACITY_NAME,  
       L_LMTN.LIMITATION_NAME L_LMTN_LIMITATION_NAME,  
       L_CPCT.STATUS L_CPCT_STATUS,  
       L_GRDE.GRADE_SEQUENCE L_GRDE_GRADE_SEQUENCE  
FROM SICD_POSITIONS PSTN,  
     SICD_CAPACITIES L_CPCT,  
     SICD_GRADES L_GRDE,  
     SICD_LIMITATIONS L_LMTN  
WHERE PSTN.CAPACITY_ID = L_CPCT.CAPACITY_ID AND  
      PSTN.GRADE_ID = L_GRDE.GRADE_ID AND  
      PSTN.LIMITATION_ID = L_LMTN.LIMITATION_ID AND  
      /* CG$LOVI_WC_START LOV_POSITIONS 230 */  
      (PSTN.grade_id IN (SELECT UNIQUE p.grade_id FROM SICD_POSITIONS p ,SICD_CAPACITIES c   
                          WHERE p.capacity_id = c.capacity_id AND c.capacity_type = 'SQC'))  
      /* CG$LOVI_WC_END LOV_POSITIONS 230 */  
ORDER BY L_CPCT_CAPACITY_SEQUENCE]]></SQLQuery>
