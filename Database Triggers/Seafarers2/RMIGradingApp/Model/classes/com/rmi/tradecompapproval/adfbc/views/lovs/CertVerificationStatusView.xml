<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="CertVerificationStatusView"
  Version="12.1.3.10.8"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="com.rmi.tradecompapproval.adfbc.views.lovs.CertVerificationStatusView_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="pVerifyType"
    Kind="where"
    Type="java.lang.String">
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYHINT
          Value="Hide"/>
      </SchemaBasedProperties>
    </Properties>
  </Variable>
  <SQLQuery><![CDATA[SELECT 'P' VERICATION_CODE,    
'Pending' VERICATION_DESC    
FROM DUAL    
UNION ALL  
SELECT 'W' VERICATION_CODE,    
'Waiting on Response' VERICATION_DESC    
FROM DUAL  
WHERE 'W' = :pVerifyType  
UNION ALL    
SELECT 'Y' VERICATION_CODE,    
'Yes' VERICATION_DESC    
FROM DUAL   
UNION ALL    
SELECT 'N' VERICATION_CODE,    
'No' VERICATION_DESC    
FROM DUAL]]></SQLQuery>
  <ViewAttribute
    Name="VericationCode"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="255"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="VERICATION_CODE"
    Expression="VERICATION_CODE"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="1"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="VericationDesc"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="7"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="VERICATION_DESC"
    Expression="VERICATION_DESC"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="7"/>
    </DesignTime>
  </ViewAttribute>
  <AttrArray Name="KeyAttributes">
    <Item Value="VericationCode"/>
  </AttrArray>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="com.rmi.tradecompapproval.adfbc.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
