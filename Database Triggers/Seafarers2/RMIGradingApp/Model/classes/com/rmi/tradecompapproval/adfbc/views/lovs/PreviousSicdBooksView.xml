<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="PreviousSicdBooksView"
  Version="12.1.3.10.8"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="com.rmi.tradecompapproval.adfbc.views.lovs.PreviousSicdBooksView_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="pSeafarerId"
    Kind="where"
    Type="java.lang.Integer">
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYHINT
          Value="Hide"/>
      </SchemaBasedProperties>
    </Properties>
  </Variable>
  <Variable
    Name="pDocumentId"
    Kind="where"
    Type="java.lang.Integer">
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYHINT
          Value="Hide"/>
      </SchemaBasedProperties>
    </Properties>
  </Variable>
  <SQLQuery><![CDATA[SELECT DCMT.DOCUMENT_ID DOCUMENT_ID,   
       DCMT.BOOK_NUMBER BOOK_NUMBER   
FROM SICD_DOCUMENTS DCMT   
WHERE /* CG$LOVI_WC_START LOV_BOOK 50 */   
      (dcmt.document_type = 'BOOK'    
        and dcmt.seafarer_id = :pSeafarerId   
        and dcmt.document_id <> NVL(:pDocumentId,0)   
      )]]></SQLQuery>
  <ViewAttribute
    Name="DocumentId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.math.BigDecimal"
    ColumnType="BIGINT"
    AliasName="DOCUMENT_ID"
    Expression="DOCUMENT_ID"
    SQLType="NUMERIC">
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYHINT
          Value="Hide"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <ViewAttribute
    Name="BookNumber"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="38"
    Scale="0"
    Type="java.math.BigInteger"
    ColumnType="NUMBER"
    AliasName="BOOK_NUMBER"
    Expression="BOOK_NUMBER"
    SQLType="NUMERIC">
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYWIDTH
          Value="30"/>
        <DISPLAYHEIGHT
          Value="10"/>
        <LABEL
          ResId="com.rmi.tradecompapproval.adfbc.views.lovs.PreviousBooksLOV.BookNumber_LABEL"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <AttrArray Name="KeyAttributes">
    <Item Value="DocumentId"/>
  </AttrArray>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="com.rmi.tradecompapproval.adfbc.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
