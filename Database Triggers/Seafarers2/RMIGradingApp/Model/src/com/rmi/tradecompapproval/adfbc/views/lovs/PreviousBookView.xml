      <SchemaBasedProperties>
        <LABEL
          ResId="com.rmi.tradecompapproval.adfbc.views.lovs.PreviousBookView.Status_LABEL"/>
        <DISPLAYWIDTH
          Value="15"/>
        <DISPLAYHEIGHT
          Value="15"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <ViewAttribute
