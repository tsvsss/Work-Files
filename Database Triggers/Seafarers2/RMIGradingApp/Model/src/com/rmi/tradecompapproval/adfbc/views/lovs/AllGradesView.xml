<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="AllGradesView"
  Version="12.1.3.10.47"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  FetchMode="FETCH_AS_NEEDED"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="com.rmi.tradecompapproval.adfbc.views.lovs.AllGradesView_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="pGradeId"
    Kind="where"
    Type="java.lang.Integer"/>
  <SQLQuery><![CDATA[select grade_id, grade_code,    
grade_name||'('||grade_code||')' grade_name,     
inventory_item_id,grade_type from sicd_grades      
where grade_id = :pGradeId  
order by grade_sequence]]></SQLQuery>
  <ViewAttribute
    Name="GradeId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.lang.Integer"
    ColumnType="BIGINT"
    AliasName="GRADE_ID"
    Expression="GRADE_ID"
    SQLType="NUMERIC"/>
  <ViewAttribute
    Name="GradeCode"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="10"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="GRADE_CODE"
    Expression="GRADE_CODE"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="10"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="GradeName"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="72"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="GRADE_NAME"
    Expression="GRADE_NAME"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="72"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="InventoryItemId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.lang.Integer"
    ColumnType="NUMBER"
    AliasName="INVENTORY_ITEM_ID"
    Expression="INVENTORY_ITEM_ID"
    SQLType="NUMERIC"/>
  <ViewAttribute
    Name="GradeType"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="5"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="GRADE_TYPE"
    Expression="GRADE_TYPE"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="5"/>
    </DesignTime>
  </ViewAttribute>
  <AttrArray Name="KeyAttributes">
    <Item Value="GradeId"/>
  </AttrArray>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="com.rmi.tradecompapproval.adfbc.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
