<?xml version="1.0" encoding="UTF-8" ?>
<pageDefinition xmlns="http://xmlns.oracle.com/adfm/uimodel" version="12.1.3.10.47" id="MainPageDef"
                Package="com.rmi.login.view.main">
    <parameters/>
    <executables>
        <variableIterator id="variables"/>
        <iterator id="UserMenuViewIterator" Binds="UserMenuView" DataControl="RmiLoginAppModuleDataControl"
                  RangeSize="25"/>
        <taskFlow id="dynamicRegion" taskFlowId="${pageFlowScope.mainBean.dynamicTaskFlowId}" activation="deferred"
                  xmlns="http://xmlns.oracle.com/adf/controller/binding"/>
    </executables>
    <bindings>
        <tree IterBinding="UserMenuViewIterator" id="UserMenuView">
            <nodeDefinition DefName="com.rmi.login.adfbc.views.readonly.UserMenuView" Name="UserMenuView0">
                <AttrNames>
                    <Item Value="MenuTitle"/>
                    <Item Value="RoleId"/>
                </AttrNames>
                <Accessors>
                    <Item Value="UserSubMenuView"/>
                </Accessors>
            </nodeDefinition>
            <nodeDefinition DefName="com.rmi.login.adfbc.views.readonly.UserSubMenuView" Name="UserMenuView1">
                <AttrNames>
                    <Item Value="MenuItemTitle"/>
                    <Item Value="RoleId"/>
                    <Item Value="TaskflowId"/>
                </AttrNames>
            </nodeDefinition>
        </tree>
        <methodAction id="getMenuItems" RequiresUpdateModel="true" Action="invokeMethod" MethodName="getMenuItems"
                      IsViewObjectMethod="false" DataControl="RmiLoginAppModuleDataControl"
                      InstanceName="data.RmiLoginAppModuleDataControl.dataProvider"
                      ReturnName="data.RmiLoginAppModuleDataControl.methodResults.getMenuItems_RmiLoginAppModuleDataControl_dataProvider_getMenuItems_result">
            <NamedData NDName="taskflowId" NDType="java.lang.String"/>
            <NamedData NDName="roleId" NDType="java.lang.String"/>
        </methodAction>
        <methodAction id="menuOnLoad" RequiresUpdateModel="true" Action="invokeMethod" MethodName="menuOnLoad"
                      IsViewObjectMethod="false" DataControl="RmiLoginAppModuleDataControl"
                      InstanceName="data.RmiLoginAppModuleDataControl.dataProvider"
                      ReturnName="data.RmiLoginAppModuleDataControl.methodResults.menuOnLoad_RmiLoginAppModuleDataControl_dataProvider_menuOnLoad_result">
            <NamedData NDName="userId" NDType="java.lang.String"/>
        </methodAction>
        <methodAction id="getEmailNotifBody" RequiresUpdateModel="true" Action="invokeMethod"
                      MethodName="getEmailNotifBody" IsViewObjectMethod="false"
                      DataControl="RmiLoginAppModuleDataControl"
                      InstanceName="data.RmiLoginAppModuleDataControl.dataProvider"
                      ReturnName="data.RmiLoginAppModuleDataControl.methodResults.getEmailNotifBody_RmiLoginAppModuleDataControl_dataProvider_getEmailNotifBody_result">
            <NamedData NDName="emailType" NDType="java.lang.String"/>
        </methodAction>
        <methodAction id="getRoleDetails" RequiresUpdateModel="true" Action="invokeMethod" MethodName="getRoleDetails"
                      IsViewObjectMethod="false" DataControl="RmiLoginAppModuleDataControl"
                      InstanceName="data.RmiLoginAppModuleDataControl.dataProvider"
                      ReturnName="data.RmiLoginAppModuleDataControl.methodResults.getRoleDetails_RmiLoginAppModuleDataControl_dataProvider_getRoleDetails_result">
            <NamedData NDName="userId" NDType="java.lang.String"/>
        </methodAction>
        <methodAction id="getSingleRoleDetails" RequiresUpdateModel="true" Action="invokeMethod"
                      MethodName="getSingleRoleDetails" IsViewObjectMethod="false"
                      DataControl="RmiLoginAppModuleDataControl"
                      InstanceName="data.RmiLoginAppModuleDataControl.dataProvider"
                      ReturnName="data.RmiLoginAppModuleDataControl.methodResults.getSingleRoleDetails_RmiLoginAppModuleDataControl_dataProvider_getSingleRoleDetails_result">
            <NamedData NDName="userId" NDType="java.lang.String"/>
            <NamedData NDName="roleId" NDType="java.math.BigDecimal"/>
        </methodAction>
    </bindings>
</pageDefinition>
