<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="GradeOcView"
  Version="12.1.3.10.47"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full"
  FetchMode="FETCH_AS_NEEDED">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="com.rmi.applicationEntry.adfbc.views.readOnlyVO.GradeOcView_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <SQLQuery><![CDATA[SELECT   grade_id, grade_code, grade_name, a.inventory_item_id, grade_type, b.concatenated_segments item_code  
    FROM sicd_grades a, mtl_system_items_kfv b  
   WHERE grade_type = 'OC' AND status = 'Active'  
   AND a.inventory_item_id = b.inventory_item_id(+)  
   and b.organization_id(+) = 125  
ORDER BY grade_code,grade_sequence]]></SQLQuery>
