<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="OnlineConsentBySeafarerIdView"
  Version="12.1.3.10.47"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="com.rmi.applicationEntry.adfbc.views.readOnlyVO.OnlineConsentBySeafarerIdView_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="pSeafarerId"
    Kind="where"
    Type="java.lang.Integer"/>
  <SQLQuery><![CDATA[select consent_id from RMI_DATA_PRIVACY_CONSENT where entity_type = 'SEAFARERS' AND CONSENT_SOURCE = 'Online' and  sysdate between consent_date and consent_expire_date 
and entity_id = :pSeafarerId]]></SQLQuery>
  <ViewAttribute
    Name="ConsentId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.lang.Integer"
    ColumnType="NUMBER"
    AliasName="CONSENT_ID"
    Expression="CONSENT_ID"
    SQLType="NUMERIC"/>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="com.rmi.applicationEntry.adfbc.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
