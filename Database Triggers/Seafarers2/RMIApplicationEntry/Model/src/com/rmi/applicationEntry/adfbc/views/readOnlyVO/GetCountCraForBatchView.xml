<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="GetCountCraForBatchView"
  Version="12.1.3.10.47"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="com.rmi.applicationEntry.adfbc.views.readOnlyVO.GetCountCraForBatchView_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="pBatchId"
    Kind="where"
    Type="java.lang.Integer"/>
  <SQLQuery><![CDATA[select bt.esi_batch_id,(select count(*) from rmi_seafarer.rmi_seafarers_iface sea ,       
rmi_seafarer.rmi_seafarer_docs_iface docs        
where docs.esi_id = sea.esi_id and       
sea.esi_batch_id = bt.esi_batch_id and       
docs.cra_required = 'Y'    
and docs.cra_sent_date is null) count_cra from rmi_seafarer.rmi_batch_iface bt where bt.esi_batch_id = :pBatchId]]></SQLQuery>
  <ViewAttribute
    Name="EsiBatchId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.lang.Integer"
    ColumnType="NUMBER"
    AliasName="ESI_BATCH_ID"
    Expression="ESI_BATCH_ID"
    SQLType="INTEGER"/>
  <ViewAttribute
    Name="CountCra"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.lang.Integer"
    ColumnType="NUMBER"
    AliasName="COUNT_CRA"
    Expression="COUNT_CRA"
    SQLType="INTEGER"/>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="com.rmi.applicationEntry.adfbc.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
