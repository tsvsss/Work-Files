<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="BatchesView"
  Version="12.1.3.10.47"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="com.rmi.applicationEntry.adfbc.views.readOnlyVO.BatchesView_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="pUserId"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="pBatchStatus"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="pBatchName"
    Kind="where"
    Type="java.lang.Integer"/>
  <Variable
    Name="pFromDate"
    Kind="where"
    Type="oracle.jbo.domain.Date"/>
  <Variable
    Name="pToDate"
    Kind="where"
    Type="oracle.jbo.domain.Date"/>
  <Variable
    Name="pCustomer"
    Kind="where"
    Type="java.lang.Integer"/>
  <SQLQuery><![CDATA[select esi_batch_id,batch_name,batch_date,batch_status,purchase_order_number,vessel_names,            
notes,batch_submitted_date,     
(select count(*) from rmi_seafarer.rmi_seafarers_iface      
where rmi_seafarer.rmi_seafarers_iface.esi_batch_id = rmi_seafarer.rmi_batch_iface.esi_batch_id) No_of_seafarer,     
(select order_number from OE_ORDER_HEADERS_ALL   
where OE_ORDER_HEADERS_ALL.header_id = rmi_seafarer.rmi_batch_iface.oe_header_id) work_order_no     
from rmi_seafarer.rmi_batch_iface              
where created_by = :pUserId           
and batch_status = decode(:pBatchStatus,null,batch_status,'All',batch_status,:pBatchStatus)             
and batch_name = nvl(:pBatchName,batch_name)           
and to_date(batch_date) between nvl(to_date(:pFromDate),to_date(batch_date)) and nvl(to_date(:pToDate),to_date(sysdate))           
and ship_to_cust_account_id = nvl(:pCustomer,ship_to_cust_account_id)]]></SQLQuery>
