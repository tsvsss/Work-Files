  <Variable
    Name="pOrderNumber"
    Kind="where"
    Type="java.lang.Integer"/>
  <SQLQuery><![CDATA[select ord.header_id,ord.order_number,ord.order_hold,ord.order_date,ord.order_status,ord.order_total,    
ord.order_by_acct,ord.order_by_cust,ord.bill_to_acct,ord.bill_to_cust, 
(select HCA.ACCOUNT_NUMBER FROM HZ_CUST_ACCOUNTS HCA, HZ_PARTIES HP            
WHERE HCA.PARTY_ID = HP.PARTY_ID         
AND HCA.CUST_ACCOUNT_ID = bt.ship_to_cust_account_id) ship_to_acct, 
(select HP.PARTY_NAME FROM HZ_CUST_ACCOUNTS HCA, HZ_PARTIES HP            
WHERE HCA.PARTY_ID = HP.PARTY_ID         
AND HCA.CUST_ACCOUNT_ID = bt.ship_to_cust_account_id) ship_to_cust,    
ord.deliver_to_acct,ord.deliver_to_cust,  
decode((select user_type from rmi_seafarer.rmi_user where user_id = bt.created_by),  
'External','Online Application','Internal','Paper Application','Online Application') order_source  
from SICD_ORDERS2_VW ord, rmi_seafarer.rmi_batch_iface  bt   
where ord.header_id = bt.oe_header_id    
and ord.order_number = :pOrderNumber]]></SQLQuery>
