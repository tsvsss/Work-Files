<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="VesselDetailView"
  Version="12.1.3.10.8"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="com.rmi.applicationEntry.adfbc.views.readOnlyVO.VesselDetailView_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="pVesselPk"
    Kind="where"
    Type="java.lang.Integer"/>
  <Variable
    Name="pVesselImo"
    Kind="where"
    Type="java.lang.Integer"/>
  <SQLQuery><![CDATA[SELECT                     
  vessel_pk,         
  imo_number,                    
  name,             
  official_number,            
  vs.description vssl_status,                    
  vt.description vssl_type,                
  vv.gross_tons,                    
  vv.propulsion_power,     
   
  vv.engine_type,                    
  NVL (registration_date, expected_reg_date)  vssl_reg_date                  
FROM vssl_vessels vv,                    
  vssl_statuses vs,                    
  vssl_vessel_types vt                    
WHERE vessel_pk = :pVesselPk                    
AND vs.code     = vv.status                    
AND vt.code(+)  = vv.vstp_code                    
AND nvl(imo_number,-1)  = nvl(:pVesselImo, nvl(imo_number,-1))                    
AND status NOT IN ('SU', 'V', 'H','RV','I')           
ORDER BY status,                    
  NVL (registration_date, expected_reg_date) ASC]]></SQLQuery>
  <ViewAttribute
    Name="VesselPk"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.lang.Integer"
    ColumnType="NUMBER"
    AliasName="VESSEL_PK"
    Expression="VESSEL_PK"
    SQLType="INTEGER"/>
  <ViewAttribute
    Name="ImoNumber"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="38"
    Scale="0"
    Type="java.lang.Integer"
    ColumnType="NUMBER"
    AliasName="IMO_NUMBER"
    Expression="IMO_NUMBER"
    SQLType="INTEGER"/>
  <ViewAttribute
    Name="Name"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="50"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="NAME"
    Expression="NAME"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="50"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="OfficialNumber"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="38"
    Scale="0"
    Type="java.lang.Integer"
    ColumnType="NUMBER"
    AliasName="OFFICIAL_NUMBER"
    Expression="OFFICIAL_NUMBER"
    SQLType="INTEGER"/>
  <ViewAttribute
    Name="VsslStatus"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="50"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="VSSL_STATUS"
    Expression="VSSL_STATUS"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="50"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="VsslType"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="50"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="VSSL_TYPE"
    Expression="VSSL_TYPE"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="50"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="GrossTons"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="38"
    Scale="0"
    Type="java.lang.Integer"
    ColumnType="NUMBER"
    AliasName="GROSS_TONS"
    Expression="GROSS_TONS"
    SQLType="INTEGER"/>
  <ViewAttribute
    Name="PropulsionPower"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="38"
    Scale="0"
    Type="java.lang.Integer"
    ColumnType="NUMBER"
    AliasName="PROPULSION_POWER"
    Expression="PROPULSION_POWER"
    SQLType="INTEGER"/>
  <ViewAttribute
    Name="EngineType"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="30"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="ENGINE_TYPE"
    Expression="ENGINE_TYPE"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="30"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="VsslRegDate"
    IsPersistent="false"
    PrecisionRule="true"
    Type="oracle.jbo.domain.Date"
    ColumnType="DATE"
    AliasName="VSSL_REG_DATE"
    Expression="VSSL_REG_DATE"
    SQLType="DATE"/>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="com.rmi.applicationEntry.adfbc.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
