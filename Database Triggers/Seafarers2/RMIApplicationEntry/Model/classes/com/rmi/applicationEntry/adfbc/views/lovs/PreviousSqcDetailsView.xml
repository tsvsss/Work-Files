<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="PreviousSqcDetailsView"
  Version="12.1.3.10.8"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="com.rmi.applicationEntry.adfbc.views.lovs.PreviousSqcDetailsView_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="pSeafarerId"
    Kind="where"
    Type="java.lang.Integer"/>
  <Variable
    Name="pGradeId"
    Kind="where"
    Type="java.lang.Integer"/>
  <SQLQuery><![CDATA[SELECT D.DOCUMENT_ID, D.GRADE_ID, D.ENDORSEMENT_NUMBER, D.ENDORSEMENT_COUNTRY,    
D.ENDORSEMENT_ISSUE_DATE, D.ENDORSEMENT_EXPIRATION_DATE, D.DOCUMENT_BOOK_ID,    
(CASE WHEN NVL(D.EXPIRATION_DATE,(SELECT SDB.EXPIRATION_DATE FROM SICD_DOCUMENTS SDB    
                                  WHERE SDB.DOCUMENT_ID = D.DOCUMENT_BOOK_ID))    
        BETWEEN SYSDATE - 364 AND SYSDATE + 364 THEN 'N'    
    ELSE 'Y' END) DOC_CHARGEABLE    
FROM SICD_DOCUMENTS D    
WHERE D.DOCUMENT_BOOK_ID = (SELECT MAX (DOCUMENT_ID)    
                            FROM SICD_DOCUMENTS    
                           WHERE SEAFARER_ID = D.SEAFARER_ID   
                             AND DOCUMENT_TYPE = 'BOOK'    
                             AND BOOK_TYPE IN ('5YR','10YR')      
                             /* AND STATUS = 'Active' */    
                             AND EXPIRATION_DATE BETWEEN SYSDATE - 364 AND SYSDATE + 364)   
AND D.DOCUMENT_TYPE = 'SQC'    
AND D.STATUS = 'Active'   
AND D.SEAFARER_ID = :pSeafarerId    
AND D.GRADE_ID = :pGradeId]]></SQLQuery>
  <ViewAttribute
    Name="DocumentId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.math.BigInteger"
    ColumnType="BIGINT"
    AliasName="DOCUMENT_ID"
    Expression="DOCUMENT_ID"
    SQLType="NUMERIC"/>
  <ViewAttribute
    Name="GradeId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="38"
    Scale="0"
    Type="java.math.BigInteger"
    ColumnType="NUMBER"
    AliasName="GRADE_ID"
    Expression="GRADE_ID"
    SQLType="NUMERIC"/>
  <ViewAttribute
    Name="EndorsementNumber"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="60"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="ENDORSEMENT_NUMBER"
    Expression="ENDORSEMENT_NUMBER"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="60"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="EndorsementCountry"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="4"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="ENDORSEMENT_COUNTRY"
    Expression="ENDORSEMENT_COUNTRY"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="4"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="EndorsementIssueDate"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="oracle.jbo.domain.Date"
    ColumnType="DATE"
    AliasName="ENDORSEMENT_ISSUE_DATE"
    Expression="ENDORSEMENT_ISSUE_DATE"
    SQLType="DATE"/>
  <ViewAttribute
    Name="EndorsementExpirationDate"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="oracle.jbo.domain.Date"
    ColumnType="DATE"
    AliasName="ENDORSEMENT_EXPIRATION_DATE"
    Expression="ENDORSEMENT_EXPIRATION_DATE"
    SQLType="DATE"/>
  <ViewAttribute
    Name="DocumentBookId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="38"
    Scale="0"
    Type="java.math.BigInteger"
    ColumnType="NUMBER"
    AliasName="DOCUMENT_BOOK_ID"
    Expression="DOCUMENT_BOOK_ID"
    SQLType="NUMERIC"/>
  <ViewAttribute
    Name="DocChargeable"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="1"
    Type="java.lang.String"
    ColumnType="CHAR"
    AliasName="DOC_CHARGEABLE"
    Expression="DOC_CHARGEABLE"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="1"/>
    </DesignTime>
  </ViewAttribute>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="com.rmi.applicationEntry.adfbc.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
