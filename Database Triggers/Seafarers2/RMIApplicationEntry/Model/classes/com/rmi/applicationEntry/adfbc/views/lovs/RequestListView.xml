<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="RequestListView"
  Version="12.1.3.10.47"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="com.rmi.applicationEntry.adfbc.views.lovs.RequestListView_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <SQLQuery><![CDATA[select c.concurrent_program_name short_name, c.user_concurrent_program_name name, c.description, c.concurrent_program_id   
   
from fnd_request_group_units a, FND_RESPONSIBILITY_VL b, FND_CONCURRENT_PROGRAMS_VL c   
   
where a.request_group_id = b.request_group_id   
   
and   a.request_unit_id = c.concurrent_program_id   
   
and   b.responsibility_name = 'Seafarer Documentation - Super User']]></SQLQuery>
