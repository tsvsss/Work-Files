  <Variable
    Name="pEsiBatchId"
    Kind="where"
    Type="java.lang.Integer"/>
  <SQLQuery><![CDATA[select seafarer.first_name,seafarer.last_name,seafarer.birth_date,seafarer.birth_place,seafarer.seafarer_id,   
(select country_name from sicd_countries where country_code =seafarer.PASSPORT_ISSUING_COUNTRY_CODE) passport_issuing_country,         
(select country_name from sicd_countries where country_code =seafarer.NATIONALITY) RESIDENCE_COUNTRY,   
decode(ISSUE_TYPE,'Current','Not Required',ISSUE_TYPE) ISSUE_TYPE,   
(select count(*) from rmi_seafarer.rmi_seafarers_iface sea ,                    
rmi_seafarer.rmi_seafarer_docs_iface docs                     
where docs.esi_id = sea.esi_id and    
sea.esi_id = seafarer.esi_id and   
docs.cra_required = 'Y' and nvl(docs.cra_approved_flag,'NA') != 'N') cra_count,      
(select count(*) from rmi_seafarer.rmi_seafarers_iface sea ,                       
rmi_seafarer.rmi_seafarer_docs_iface docs                        
where docs.esi_id = sea.esi_id and     
sea.esi_id = seafarer.esi_id and   
docs.cra_required = 'Y'                    
and docs.cra_sent_date is null and nvl(docs.cra_approved_flag,'NA') != 'N') cra_sent_count,  
seafarer.status  
from rmi_seafarer.rmi_seafarers_iface seafarer where seafarer.esi_batch_id = :pEsiBatchId]]></SQLQuery>
