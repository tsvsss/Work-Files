  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="com.rmi.applicationEntry.adfbc.views.readOnlyVO.FindSeafarerView_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="pName"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="pDateOfBirth"
    Kind="where"
    Type="oracle.jbo.domain.Date"/>
  <Variable
    Name="pSeafarerId"
    Kind="where"
    Type="java.lang.String"/>
  <SQLQuery><![CDATA[select sea.first_name||' '||sea.last_name sea_name,sea.esi_id,bt.esi_batch_id,trunc(bt.batch_date) batch_date,             
bt.purchase_order_number,bt.expedited_processing,bt.batch_name,             
(SELECT jrs.name                   
            FROM rmi_seafarer.rmi_user ua,jtf_rs_salesreps jrs                   
           WHERE ua.user_id =  bt.created_by                  
             AND ua.default_processing_office = to_char(jrs.salesrep_id)) region,             
(select order_number from OE_ORDER_HEADERS_ALL                  
where OE_ORDER_HEADERS_ALL.header_id = bt.oe_header_id) work_order_no,           
bt.expedited_processing processing,     
'workflow status' workflow_status,   
bt.batch_submitted_date   
from rmi_seafarer.rmi_seafarers_iface sea,rmi_seafarer.rmi_batch_iface bt             
where sea.esi_batch_id = bt.esi_batch_id             
and (upper(NVL(sea.last_name,'Xx')) like upper(:pName)                 
                      OR NVL(sea.birth_date,Sysdate-900) =                  
                                 :pDateOfBirth      
                      OR NVL(sea.seafarer_id,-99999999) =                  
                                 :pSeafarerId               
                                           )]]></SQLQuery>
