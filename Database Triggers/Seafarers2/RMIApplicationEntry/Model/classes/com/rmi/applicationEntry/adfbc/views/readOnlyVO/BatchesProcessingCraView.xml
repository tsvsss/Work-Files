    Name="pBatchId"
    Kind="where"
    Type="java.lang.Integer"/>
  <SQLQuery><![CDATA[SELECT esi_batch_id,  bill_to_cust_account_id,  bill_to_site_use_id,         
  bill_to_org_contact_id,  batch_name,  esi_id,  seafarer_id,  esdi_id,         
  grade_id,  creation_date,  created_by,         
  (select user_name from rmi_seafarer.rmi_user where user_id = rmi_pending_cra.created_by)created_by_user,  cra_approved_date,  certificate_type,  origin,  document_id,         
  (SELECT a.party_name         
  FROM hz_parties a,         
    hz_cust_accounts b         
  WHERE a.party_id      = b.party_id         
  AND b.cust_account_id = rmi_pending_cra.bill_to_cust_account_id) customer_name,         
  (SELECT first_name         
    || ' '         
    || last_name         
  FROM rmi_seafarer.rmi_seafarers_iface         
  WHERE esi_id = rmi_pending_cra.esi_id         
  AND origin   = 'External'     
  UNION ALL         
  SELECT first_name         
    || ' '         
    || last_name         
  FROM sicd_seafarers         
  WHERE seafarer_id = rmi_pending_cra.seafarer_id         
  AND origin   = 'Internal'         
  ) seafarer_name,         
  (SELECT grade_name         
  FROM sicd_grades         
  WHERE grade_id = rmi_pending_cra.grade_id         
  AND status     = 'Active'         
  ) grade_name,  
  substr(RMI_SICD_PUB.get_seafarer_wc_status(seafarer_id),1,instr(RMI_SICD_PUB.get_seafarer_wc_status(seafarer_id),',')-1) tc_status  
FROM rmi_pending_cra     
where esi_batch_id = nvl(:pBatchId,esi_batch_id)]]></SQLQuery>
