<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="GetCategoryIdByCertificateTypeView"
  Version="12.1.3.10.47"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="com.rmi.applicationEntry.adfbc.views.readOnlyVO.GetCategoryIdByCertificateTypeView_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="pCertificateType"
    Kind="where"
    Type="java.lang.String"/>
  <SQLQuery><![CDATA[SELECT category_id FROM iri_edocs_categories WHERE CATEGORY = 'SICD'  AND enabled_flag    = 'Y'  
AND document_type   = 'MI Docs' AND document_code   = decode(:pCertificateType,'CRA','0027','UA','0024','0027')]]></SQLQuery>
  <ViewAttribute
    Name="CategoryId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.lang.Integer"
    ColumnType="NUMBER"
    AliasName="CATEGORY_ID"
    Expression="CATEGORY_ID"
    SQLType="NUMERIC"/>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="com.rmi.applicationEntry.adfbc.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
