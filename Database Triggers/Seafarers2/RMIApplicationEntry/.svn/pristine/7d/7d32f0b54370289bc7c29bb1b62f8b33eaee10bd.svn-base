<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="PendingBatchesView"
  Version="12.1.3.10.47"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="com.rmi.applicationEntry.adfbc.views.readOnlyVO.PendingBatchesView_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="pUserId"
    Kind="where"
    Type="java.lang.Integer"/>
  <SQLQuery><![CDATA[select esi_batch_id,batch_name,trunc(batch_date) batch_date,batch_status,purchase_order_number,vessel_names,                           
notes,trunc(batch_submitted_date) batch_submitted_date,                    
(select count(*) from rmi_seafarer.rmi_seafarers_iface                     
where rmi_seafarer.rmi_seafarers_iface.esi_batch_id = rmi_seafarer.rmi_batch_iface.esi_batch_id) No_of_seafarer,                    
(select order_number from OE_ORDER_HEADERS_ALL                  
where OE_ORDER_HEADERS_ALL.header_id = rmi_seafarer.rmi_batch_iface.oe_header_id) work_order_no,      
decode(expedited_processing,'N','false','Y','true','false') expedited_processing_pending       
from rmi_seafarer.rmi_batch_iface                             
where created_by = :pUserId                          
and batch_status = 'Pending'               
union             
select esi_batch_id,batch_name,trunc(batch_date) batch_date,batch_status,purchase_order_number,vessel_names,                           
notes,trunc(batch_submitted_date) batch_submitted_date,                    
(select count(*) from rmi_seafarer.rmi_seafarers_iface                     
where rmi_seafarer.rmi_seafarers_iface.esi_batch_id = rmi_seafarer.rmi_batch_iface.esi_batch_id) No_of_seafarer,                    
(select order_number from OE_ORDER_HEADERS_ALL                  
where OE_ORDER_HEADERS_ALL.header_id = rmi_seafarer.rmi_batch_iface.oe_header_id) work_order_no,      
decode(expedited_processing,'N','false','Y','true','false') expedited_processing_pending      
from rmi_seafarer.rmi_batch_iface                             
where  
(deliver_TO_CUST_ACCOUNT_ID  IN (SELECT ordering_agent_account_id FROM rmi_seafarer.rmi_user_order_agent_map WHERE USER_ID=:pUserId)   
or (select user_type from rmi_seafarer.rmi_user where user_id = :pUserId) = 'Internal') 
and batch_status = 'Pending'              
and 'My Companies Orders' = (select view_orders from rmi_seafarer.rmi_user where USER_ID = :pUserId)  
and ((select user_type from rmi_seafarer.rmi_user where user_id = :pUserId) = (select user_type from rmi_seafarer.rmi_user where user_id = rmi_seafarer.rmi_batch_iface.created_by))  
order by batch_date asc]]></SQLQuery>
