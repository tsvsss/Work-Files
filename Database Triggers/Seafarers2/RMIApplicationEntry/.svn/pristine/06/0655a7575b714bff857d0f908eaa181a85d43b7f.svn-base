  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
    <Attr Name="_codeGenFlag2" Value="Access"/>
    <Attr Name="_isCodegen" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="com.rmi.applicationEntry.adfbc.views.readOnlyVO.SeafarerListTcView_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="pEsiBatchId"
    Kind="where"
    Type="java.lang.Integer"/>
  <SQLQuery><![CDATA[select seafarer.first_name,seafarer.last_name,seafarer.birth_date,seafarer.seafarer_id,     
(select count(*) from rmi_seafarer.rmi_seafarers_iface sea ,                      
rmi_seafarer.rmi_seafarer_docs_iface docs                       
where docs.esi_id = sea.esi_id and      
sea.esi_id = seafarer.esi_id and     
docs.cra_required = 'Y' and nvl(docs.cra_approved_flag,'NA') != 'N') cra_count,        
(select count(*) from rmi_seafarer.rmi_seafarers_iface sea ,                         
rmi_seafarer.rmi_seafarer_docs_iface docs                          
where docs.esi_id = sea.esi_id and       
sea.esi_id = seafarer.esi_id and     
docs.cra_required = 'Y'                      
and docs.cra_sent_date is null and nvl(docs.cra_approved_flag,'NA') != 'N') cra_sent_count,   
NVL(REGEXP_SUBSTR(RMI_SICD_PUB.get_seafarer_wc_status(seafarer.seafarer_id), '[^,]+'),  
                         'Not Available') tc_status,  
                         to_date(NVL(REGEXP_SUBSTR(RMI_SICD_PUB.get_seafarer_wc_status(seafarer.seafarer_id), '[^,]+',1,2),  
                         NULL),'dd-MON-yyyy') tc_status_date  
from rmi_seafarer.rmi_seafarers_iface seafarer where seafarer.esi_batch_id = :pEsiBatchId]]></SQLQuery>
