<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE html>
<f:view xmlns:f="http://java.sun.com/jsf/core" xmlns:af="http://xmlns.oracle.com/adf/faces/rich">
    <af:document title="Seafarer TC Details" id="d1">
        <af:messages id="m1"/>
        <af:form id="f1">
            <af:panelGroupLayout id="pgl1" styleClass="AFStretchWidth">
                <af:panelCollection id="pc1" styleClass="AFStretchWidth">
                    <f:facet name="menus"/>
                    <f:facet name="toolbar"/>
                    <af:table value="#{bindings.SeafarerListTcView.collectionModel}" var="row"
                              rows="#{bindings.SeafarerListTcView.rangeSize}"
                              emptyText="#{bindings.SeafarerListTcView.viewable ? 'No data to display.' : 'Access Denied.'}"
                              rowBandingInterval="0"
                              selectedRowKeys="#{bindings.SeafarerListTcView.collectionModel.selectedRow}"
                              selectionListener="#{bindings.SeafarerListTcView.collectionModel.makeCurrent}"
                              rowSelection="single" fetchSize="#{bindings.SeafarerListTcView.rangeSize}"
                              filterModel="#{bindings.SeafarerListTcViewQuery.queryDescriptor}"
                              queryListener="#{bindings.SeafarerListTcViewQuery.processQuery}" filterVisible="true"
                              varStatus="vs" id="t1" columnStretching="column:c2" autoHeightRows="5"
                              contentDelivery="immediate" styleClass="AFStretchWidth">
                        <af:column sortProperty="#{bindings.SeafarerListTcView.hints.LastName.name}" filterable="true"
                                   sortable="true" headerText="Surname" id="c2" width="180">
                            <af:outputText value="#{row.LastName}"
                                           shortDesc="#{bindings.SeafarerListTcView.hints.LastName.tooltip}" id="ot2"
                                           inlineStyle="color:#{row.tranFinalCra == -1 ? 'Red' : row.tranFinalCra == 1 ? 'Green' : ''}; "/>
                        </af:column>
                        <af:column sortProperty="#{bindings.SeafarerListTcView.hints.FirstName.name}" filterable="true"
                                   sortable="true" headerText="Given Name" id="c1" width="180">
                            <af:outputText value="#{row.FirstName}"
                                           shortDesc="#{bindings.SeafarerListTcView.hints.FirstName.tooltip}" id="ot1"
                                           inlineStyle="color:#{row.tranFinalCra == -1 ? 'Red' : row.tranFinalCra == 1 ? 'Green' : ''}; "/>
                        </af:column>
                        <af:column sortProperty="#{bindings.SeafarerListTcView.hints.BirthDate.name}" filterable="true"
                                   sortable="true" headerText="Birth Date" id="c3">
                            <f:facet name="filter">
                                <af:inputDate value="#{bindings.SeafarerListTcViewQuery.queryDescriptor.filterConjunctionCriterion.criterionMap.BirthDate.value}"
                                              id="id1">
                                    <af:convertDateTime pattern="#{bindings.SeafarerListTcView.hints.BirthDate.format}"/>
                                </af:inputDate>
                            </f:facet>
                            <af:outputText value="#{row.BirthDate}"
                                           shortDesc="#{bindings.SeafarerListTcView.hints.BirthDate.tooltip}" id="ot3"
                                           inlineStyle="color:#{row.tranFinalCra == -1 ? 'Red' : row.tranFinalCra == 1 ? 'Green' : ''}; text-transform:uppercase;">
                                <af:convertDateTime pattern="dd-MMM-yyyy"/>
                            </af:outputText>
                        </af:column>
                        <af:column sortProperty="#{bindings.SeafarerListTcView.hints.TcStatus.name}" filterable="true"
                                   sortable="true" headerText="TC Status" id="c4" width="130">
                            <af:outputText value="#{row.TcStatus}"
                                           shortDesc="#{bindings.SeafarerListTcView.hints.TcStatus.tooltip}" id="ot4"
                                           inlineStyle="color:#{row.tranFinalCra == -1 ? 'Red' : row.tranFinalCra == 1 ? 'Green' : ''}; "/>
                        </af:column>
                        <af:column sortProperty="#{bindings.SeafarerListTcView.hints.TcStatusDate.name}"
                                   filterable="true" sortable="true" headerText="TC Status Date" id="c5" width="120">
                            <f:facet name="filter">
                                <af:inputDate value="#{bindings.SeafarerListTcViewQuery.queryDescriptor.filterConjunctionCriterion.criterionMap.TcStatusDate.value}"
                                              id="id2">
                                    <af:convertDateTime pattern="#{bindings.SeafarerListTcView.hints.TcStatusDate.format}"/>
                                </af:inputDate>
                            </f:facet>
                            <af:outputText value="#{row.TcStatusDate}"
                                           shortDesc="#{bindings.SeafarerListTcView.hints.TcStatusDate.tooltip}"
                                           id="ot5"
                                           inlineStyle="color:#{row.tranFinalCra == -1 ? 'Red' : row.tranFinalCra == 1 ? 'Green' : ''}; text-transform:uppercase;">
                                <af:convertDateTime pattern="dd-MMM-yyyy"/>
                            </af:outputText>
                        </af:column>
                        <af:column sortProperty="#{bindings.SeafarerListTcView.hints.tranFinalCra.name}"
                                   filterable="true" sortable="true"
                                   headerText="#{bindings.SeafarerListTcView.hints.tranFinalCra.label}" id="c6"
                                   visible="false">
                            <af:outputText value="#{row.tranFinalCra}"
                                           shortDesc="#{bindings.SeafarerListTcView.hints.tranFinalCra.tooltip}"
                                           id="ot6">
                                <af:convertNumber groupingUsed="false"
                                                  pattern="#{bindings.SeafarerListTcView.hints.tranFinalCra.format}"/>
                            </af:outputText>
                        </af:column>
                    </af:table>
                    <f:facet name="statusbar"/>
                </af:panelCollection>
            </af:panelGroupLayout>
        </af:form>
    </af:document>
</f:view>