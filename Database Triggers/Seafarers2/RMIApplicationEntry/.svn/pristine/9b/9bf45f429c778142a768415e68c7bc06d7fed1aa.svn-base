  <SQLQuery><![CDATA[select wf.esi_batch_id,bt.batch_status,trunc(bt.batch_date) batch_date,bt.batch_name,        
bt.purchase_order_number, bt.batch_submitted_date, bt.vessel_names,          
(SELECT hp_ship.party_name party_name                         
  FROM hz_parties hp_ship, hz_cust_accounts hca                         
 WHERE 1 = 1                         
   AND hca.account_name = 'SICD'                         
   AND hca.party_id = hp_ship.party_id                     
   and hca.cust_account_id = bt.deliver_to_cust_account_id) ordering_customer_name,                  
  (SELECT hp_ship.party_name party_name                         
  FROM hz_parties hp_ship, hz_cust_accounts hca                         
 WHERE 1 = 1                         
   AND hca.account_name = 'SICD'                         
   AND hca.party_id = hp_ship.party_id                     
   and hca.cust_account_id = bt.bill_to_cust_account_id) paying_customer_name,              
   (SELECT hp_ship.party_name party_name                         
  FROM hz_parties hp_ship, hz_cust_accounts hca                         
 WHERE 1 = 1                         
   AND hca.account_name = 'SICD'                         
   AND hca.party_id = hp_ship.party_id                     
   and hca.cust_account_id = bt.ship_to_cust_account_id) shipping_customer_name,         
(select count(esi_id) from rmi_seafarer.rmi_seafarers_iface                               
where esi_batch_id = bt.esi_batch_id) no_of_seafarer,                              
(SELECT jrs.name                              
            FROM rmi_seafarer.rmi_user ua,jtf_rs_salesreps jrs                              
           WHERE ua.user_id =  bt.created_by                             
             AND ua.default_processing_office = to_char(jrs.salesrep_id)) region ,                           
             (select count(*) from rmi_seafarer.rmi_seafarers_iface sea ,                              
rmi_seafarer.rmi_seafarer_docs_iface docs                               
where docs.esi_id = sea.esi_id and                              
sea.esi_batch_id = bt.esi_batch_id and                              
docs.cra_required = 'Y'              
and nvl(docs.cra_approved_flag,'NA') != 'N') cra_count,                         
(select count(*) from rmi_seafarer.rmi_seafarers_iface sea ,                               
rmi_seafarer.rmi_seafarer_docs_iface docs                                
where docs.esi_id = sea.esi_id and                               
sea.esi_batch_id = bt.esi_batch_id and                               
docs.cra_required = 'Y'                            
and docs.cra_sent_date is null and nvl(docs.cra_approved_flag,'NA') != 'N') cra_sent_count,                        
(select user_type from rmi_seafarer.rmi_user                         
where user_id = bt.created_by) order_source,                      
(select order_number from OE_ORDER_HEADERS_ALL                                 
where OE_ORDER_HEADERS_ALL.header_id = bt.oe_header_id) work_order_no,                     
wf.batch_status wf_status,                    
bt.oe_header_id ,     
decode(wf.batch_status,'Pending_TC','Pending Validation','Pending_VAL','Pending Evaluation',                
'Pending_QA','Pending QA','Pending_Shipping','Pending Shipping','Closed','Closed','Pending_ABN','Pending Assign Book Number','Rejected','Sent for correction','Unverified','Sent for correction','Pending_MTC','Pending Trade Compliance','Submitted') workflow_status,    
bt.expedited_processing,  
(select first_name||' '||last_name from rmi_seafarer.rmi_user where user_id = wf.assigned_to) user_assigned  
from                               
   rmi_seafarer.rmi_workflow wf                              
  ,rmi_seafarer.rmi_batch_iface bt                               
where                              
wf.esi_batch_id = bt.esi_batch_id                              
and wf.assigned_to = decode(:pUserId,-1,wf.assigned_to,:pUserId)                              
and wf.LAST_UPDATED_ROW='Y'        
and wf.batch_status != 'Closed'      
order by DECODE(expedited_processing,'Y',1,0) desc,cra_sent_count desc, work_order_no asc, batch_submitted_date asc,esi_batch_id asc]]></SQLQuery>
