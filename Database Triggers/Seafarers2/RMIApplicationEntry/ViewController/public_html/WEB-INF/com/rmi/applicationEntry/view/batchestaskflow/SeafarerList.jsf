<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE html>
<f:view xmlns:f="http://java.sun.com/jsf/core" xmlns:af="http://xmlns.oracle.com/adf/faces/rich">
    <af:document title="Seafarer List" id="d1">
        <af:messages id="m1"/>
        <af:form id="f1">
            <af:panelGroupLayout id="pgl1" styleClass="AFStretchWidth">
                <af:panelCollection id="pc1" styleClass="AFStretchWidth">
                    <f:facet name="menus"/>
                    <f:facet name="toolbar"/>
                    <af:table value="#{bindings.SeafarerListView.collectionModel}" var="row"
                              rows="#{bindings.SeafarerListView.rangeSize}"
                              emptyText="#{bindings.SeafarerListView.viewable ? 'No data to display.' : 'Access Denied.'}"
                              rowBandingInterval="0"
                              selectedRowKeys="#{bindings.SeafarerListView.collectionModel.selectedRow}"
                              selectionListener="#{bindings.SeafarerListView.collectionModel.makeCurrent}"
                              rowSelection="single" fetchSize="#{bindings.SeafarerListView.rangeSize}"
                              filterModel="#{bindings.SeafarerListViewQuery.queryDescriptor}"
                              queryListener="#{bindings.SeafarerListViewQuery.processQuery}" filterVisible="true"
                              varStatus="vs" id="t1" autoHeightRows="6" contentDelivery="immediate"
                              columnStretching="column:c2">
                        <af:column sortProperty="#{bindings.SeafarerListView.hints.FirstName.name}" filterable="true"
                                   sortable="true" headerText="Given Name"
                                   id="c1" width="150">
                            <af:outputText value="#{row.FirstName}"
                                           shortDesc="#{bindings.SeafarerListView.hints.FirstName.tooltip}" id="ot1"
                                           inlineStyle="color:#{row.tranFinalCra == -1 ? 'Red' : row.tranFinalCra == 1 ? 'Green' : ''}; "/>
                        </af:column>
                        <af:column sortProperty="#{bindings.SeafarerListView.hints.LastName.name}" filterable="true"
                                   sortable="true" headerText="Surname"
                                   id="c2" width="150">
                            <af:outputText value="#{row.LastName}"
                                           shortDesc="#{bindings.SeafarerListView.hints.LastName.tooltip}" id="ot2"
                                           inlineStyle="color:#{row.tranFinalCra == -1 ? 'Red' : row.tranFinalCra == 1 ? 'Green' : ''}; "/>
                        </af:column>
                        <af:column sortProperty="#{bindings.SeafarerListView.hints.BirthDate.name}" filterable="true"
                                   sortable="true" headerText="Date Of Birth"
                                   id="c3">
                            <f:facet name="filter">
                                <af:inputDate value="#{bindings.SeafarerListViewQuery.queryDescriptor.filterConjunctionCriterion.criterionMap.BirthDate.value}"
                                              id="id1">
                                    <af:convertDateTime pattern="#{bindings.SeafarerListView.hints.BirthDate.format}"/>
                                </af:inputDate>
                            </f:facet>
                            <af:outputText value="#{row.BirthDate}"
                                           shortDesc="#{bindings.SeafarerListView.hints.BirthDate.tooltip}" id="ot3"
                                           inlineStyle="color:#{row.tranFinalCra == -1 ? 'Red' : row.tranFinalCra == 1 ? 'Green' : ''}; text-transform:uppercase;">
                                <af:convertDateTime pattern="dd-MMM-yyyy"/>
                            </af:outputText>
                        </af:column>
                        <af:column sortProperty="#{bindings.SeafarerListView.hints.SeafarerId.name}" filterable="true"
                                   sortable="true" headerText="Seafarer ID"
                                   id="c4" width="90">
                            <af:outputText value="#{row.SeafarerId}"
                                           shortDesc="#{bindings.SeafarerListView.hints.SeafarerId.tooltip}" id="ot4"
                                           inlineStyle="color:#{row.tranFinalCra == -1 ? 'Red' : row.tranFinalCra == 1 ? 'Green' : ''}; ">
                                <af:convertNumber groupingUsed="false"
                                                  pattern="#{bindings.SeafarerListView.hints.SeafarerId.format}"/>
                            </af:outputText>
                        </af:column>
                        <af:column sortProperty="#{bindings.SeafarerListView.hints.ResidenceCountry.name}"
                                   filterable="true" sortable="true"
                                   headerText="Residence Country" id="c5" width="220">
                            <af:outputText value="#{row.ResidenceCountry}"
                                           shortDesc="#{bindings.SeafarerListView.hints.ResidenceCountry.tooltip}"
                                           id="ot5"
                                           inlineStyle="color:#{row.tranFinalCra == -1 ? 'Red' : row.tranFinalCra == 1 ? 'Green' : ''}; "/>
                        </af:column>
                        <af:column sortProperty="#{bindings.SeafarerListView.hints.IssueType.name}" filterable="true"
                                   sortable="true" headerText="Seafarer's Book"
                                   id="c6" width="90">
                            <af:outputText value="#{row.IssueType}"
                                           shortDesc="#{bindings.SeafarerListView.hints.IssueType.tooltip}" id="ot6"
                                           inlineStyle="color:#{row.tranFinalCra == -1 ? 'Red' : row.tranFinalCra == 1 ? 'Green' : ''}; "/>
                        </af:column>
                        <af:column headerText="Status" id="c8" width="150">
                            <af:outputText value="#{row.Status}"
                                           shortDesc="#{bindings.SeafarerListView.hints.Status.tooltip}" id="ot8"
                                           inlineStyle="color:#{row.tranFinalCra == -1 ? 'Red' : row.tranFinalCra == 1 ? 'Green' : ''}; "/>
                        </af:column>
                        <af:column sortProperty="#{bindings.SeafarerListView.hints.tranFinalCra.name}" filterable="true"
                                   sortable="true" headerText="#{bindings.SeafarerListView.hints.tranFinalCra.label}"
                                   id="c7" visible="false">
                            <af:outputText value="#{row.tranFinalCra}"
                                           shortDesc="#{bindings.SeafarerListView.hints.tranFinalCra.tooltip}" id="ot7">
                                <af:convertNumber groupingUsed="false"
                                                  pattern="#{bindings.SeafarerListView.hints.tranFinalCra.format}"/>
                            </af:outputText>
                        </af:column>
                    </af:table>
                    <f:facet name="statusbar"/>
                </af:panelCollection>
            </af:panelGroupLayout>
        </af:form>
    </af:document>
</f:view>