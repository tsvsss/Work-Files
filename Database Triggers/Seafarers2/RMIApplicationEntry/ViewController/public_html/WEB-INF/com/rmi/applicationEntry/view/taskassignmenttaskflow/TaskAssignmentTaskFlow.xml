<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="taskflow_taskAssignment">
    <default-activity>onLoadTaskAssignmentpage</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <input-parameter-definition id="__7">
      <name>param_user_id</name>
      <value>#{sessionScope.param_user_id}</value>
      <class>java.lang.Integer</class>
    </input-parameter-definition>
    <managed-bean id="__1">
      <managed-bean-name>WorkflowBean</managed-bean-name>
      <managed-bean-class>com.rmi.applicationEntry.view.taskassignmenttaskflow.WorkflowBean</managed-bean-class>
      <managed-bean-scope>pageFlow</managed-bean-scope>
    </managed-bean>
    <view id="taskAssignment">
      <page>/WEB-INF/com/rmi/applicationEntry/view/taskassignmenttaskflow/taskAssignment.jsff</page>
    </view>
    <method-call id="onLoadTaskAssignmentpage">
      <method>#{bindings.onLoadTaskAssignmentpage.execute}</method>
      <outcome>
        <fixed-outcome>onLoadTaskAssignmentpage</fixed-outcome>
      </outcome>
    </method-call>
    <task-flow-call id="SeafarerListTaskFlow">
      <task-flow-reference>
        <document>/WEB-INF/com/rmi/applicationEntry/view/batchestaskflow/SeafarerListTaskFlow.xml</document>
        <id>SeafarerListTaskFlow</id>
      </task-flow-reference>
      <input-parameter id="__11">
        <name>pEsiBatchId</name>
        <value>#{data.com_rmi_applicationEntry_view_taskAssignmentPageDef.EsiBatchId.inputValue}</value>
      </input-parameter>
      <run-as-dialog>
        <display-type>
          <inline-popup/>
        </display-type>
      </run-as-dialog>
    </task-flow-call>
    <control-flow-rule id="__2">
      <from-activity-id>taskAssignment</from-activity-id>
      <control-flow-case id="__10">
        <from-outcome>seafarerList</from-outcome>
        <to-activity-id>SeafarerListTaskFlow</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__8">
      <from-activity-id>onLoadTaskAssignmentpage</from-activity-id>
      <control-flow-case id="__9">
        <from-outcome>onLoadTaskAssignmentpage</from-outcome>
        <to-activity-id>taskAssignment</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
