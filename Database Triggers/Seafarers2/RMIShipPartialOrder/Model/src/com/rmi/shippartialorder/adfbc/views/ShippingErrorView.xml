      <Attr Name="_DisplaySize" Value="3000"/>
    </DesignTime>
  </ViewAttribute>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="com.rmi.shippartialorder.adfbc.ModelBundle"/>
  </ResourceBundle>
  <AttrArray Name="KeyAttributes">
    <Item Value="ErrorId"/>
  </AttrArray>
</ViewObject>
