      <Attr Name="_DisplaySize" Value="100"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="MsgData"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="3000"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="MSG_DATA"
    Expression="MSG_DATA"
    SQLType="VARCHAR">
