    Expression="DETAIL_DELIVERY_ID"
    SQLType="NUMERIC"/>
  <ViewAttribute
    Name="TrackingNumber"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="100"
    Type="java.lang.String"
