  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="com.rmi.shippartialorder.adfbc.views.ShippingErrorView_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="pHeaderId"
    Kind="where"
    Type="java.lang.Integer"/>
