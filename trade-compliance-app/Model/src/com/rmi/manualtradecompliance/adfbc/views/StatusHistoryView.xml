  <SQLQuery><![CDATA[SELECT  WCH.WC_REQUEST_APPROVAL_HISTORY_ID,WCH.WC_SCREENING_REQUEST_ID,WCH.STATUS,WCH.STATUS_DATE,   
WCH.STATUS_UPDATED_BY, WCH.CREATED_BY,  
WCH.CREATION_DATE,WCH.LAST_UPDATED_BY,WCH.LAST_UPDATE_DATE,WCH.LAST_UPDATE_LOGIN,   
 NVL ( ( Select user_name from RMI_SEAFARER.RMI_USER where user_id= WCH.STATUS_UPDATED_BY),   
     (Select user_name from fnd_user where user_id= WCH.STATUS_UPDATED_BY)) StatusUpdatedByName,   
     NVL ( ( Select user_name from RMI_SEAFARER.RMI_USER where user_id= WCH.LAST_UPDATED_BY),   
     (Select user_name from fnd_user where user_id= WCH.LAST_UPDATED_BY)) LastUpdatedByName   
FROM WC_REQUEST_APPROVAL_HISTORY   WCH   
WHERE WC_SCREENING_REQUEST_ID=:pWcScreeningBV     
ORDER BY LAST_UPDATE_DATE DESC]]></SQLQuery>
