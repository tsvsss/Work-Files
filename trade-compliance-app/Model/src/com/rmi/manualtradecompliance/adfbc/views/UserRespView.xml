<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="UserRespView"
  Version="12.1.3.10.47"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="com.rmi.manualtradecompliance.adfbc.views.UserRespView_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="pUserName"
    Kind="where"
    Type="java.lang.String"/>
  <SQLQuery><![CDATA[SELECT fu.user_name                "User Name",         
       frt.responsibility_name     "Responsibility Name",         
       furg.start_date             "Start Date",         
       furg.end_date               "End Date",               
       fr.responsibility_key       "Responsibility Key",         
       fa.application_short_name   "Application Short Name",        
       frt.responsibility_id        
  FROM fnd_user_resp_groups_direct        furg,         
       applsys.fnd_user                   fu,         
       applsys.fnd_responsibility_tl      frt,         
       applsys.fnd_responsibility         fr,         
       applsys.fnd_application_tl         fat,         
       applsys.fnd_application            fa         
 WHERE furg.user_id             =  fu.user_id         
   AND furg.responsibility_id   =  frt.responsibility_id         
   AND fr.responsibility_id     =  frt.responsibility_id         
   AND fa.application_id        =  fat.application_id         
   AND fr.application_id        =  fat.application_id         
   AND frt.language             =  USERENV('LANG')         
   AND UPPER(fu.user_name)      =  UPPER(:pUserName)           
   AND (furg.end_date IS NULL OR furg.end_date > TRUNC(SYSDATE))         
 ORDER BY frt.responsibility_name]]></SQLQuery>
  <ViewAttribute
    Name="UserName"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="255"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName='"User Name"'
    Expression='"User Name"'
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="100"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="ResponsibilityName"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="100"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName='"Responsibility Name"'
    Expression='"Responsibility Name"'
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="100"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="StartDate"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.sql.Timestamp"
    ColumnType="DATE"
    AliasName='"Start Date"'
    Expression='"Start Date"'
    SQLType="DATE"/>
  <ViewAttribute
    Name="EndDate"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.sql.Timestamp"
    ColumnType="DATE"
    AliasName='"End Date"'
    Expression='"End Date"'
    SQLType="DATE"/>
  <ViewAttribute
    Name="ResponsibilityKey"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="30"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName='"Responsibility Key"'
    Expression='"Responsibility Key"'
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="30"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="ApplicationShortName"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="50"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName='"Application Short Name"'
    Expression='"Application Short Name"'
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="50"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="ResponsibilityId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.math.BigDecimal"
    ColumnType="NUMBER"
    AliasName="RESPONSIBILITY_ID"
    Expression="RESPONSIBILITY_ID"
    SQLType="NUMERIC"/>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="com.rmi.manualtradecompliance.adfbc.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
