<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="DocumentLinkView"
  Version="12.1.3.10.47"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="com.rmi.manualtradecompliance.adfbc.views.DocumentLinkView_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="pEdocId"
    Kind="where"
    Type="java.lang.Integer">
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYHINT
          Value="Hide"/>
      </SchemaBasedProperties>
    </Properties>
  </Variable>
  <Variable
    Name="pCategory"
    Kind="where"
    Type="java.lang.String">
    <TransientExpression
      trustMode="untrusted"
      Name="expression"><![CDATA[adf.context.pageFlowScope.DPC_CATEGORY]]></TransientExpression>
  </Variable>
  <SQLQuery><![CDATA[select iri_html.get_url(:pCategory)||    
iri_edocs_pkg.get_edoc_url(:pEdocId) doc_link     
from dual]]></SQLQuery>
  <ViewAttribute
    Name="DocLink"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="255"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="DOC_LINK"
    Expression="DOC_LINK"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="4000"/>
    </DesignTime>
  </ViewAttribute>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="com.rmi.manualtradecompliance.adfbc.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
