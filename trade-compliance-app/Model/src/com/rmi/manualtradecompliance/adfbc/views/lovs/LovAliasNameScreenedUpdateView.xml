  <SQLQuery><![CDATA[SELECT a.full_name,   
  m.source_table_column,  
  m.source_table,  
  m.source_id,  
  a.master_id,      
  a.ID,           
  TO_DATE (m.date_of_birth, 'YYYYMMDD') dob,           
  m.IDENTIFIER,          
  m.ENTITY_TYPE,          
  UPPER(m.SEX) SEX,          
  m.ID_TYPE,          
  m.PASSPORT_NUMBER,     
  m.PASSPORT_ISSUING_COUNTRY_CODE,     
  m.CITIZENSHIP_COUNTRY_CODE,     
  m.COUNTRY_OF_RESIDENCE,   
  (select country_code from apps.sicd_countries    
where iso_alpha2_code = m.COUNTRY_OF_RESIDENCE and status='Active' and rownum = 1) COUNTRY_OF_RESIDENCE_CODE,   
(select country_name from apps.sicd_countries    
where iso_alpha2_code = m.COUNTRY_OF_RESIDENCE and status='Active' and rownum = 1) COUNTRY_OF_RESIDENCE_NAME,   
  m.CITY_OF_RESIDENCE_ID ,  
  --T20220228.0033 - Enhance WC_CITY_LIST for Start Date/End Date  
  (select CITY_NAME from WC_CITY_LIST_V       
    where WC_CITY_LIST_ID = m.CITY_OF_RESIDENCE_ID   
    --T20220302.0021 - Address Conflict is not listed under the city options for Ukraine or Russia  
    --and INTERNAL_USE_ONLY = 'N'  
    ) CITY_NAME,  
  m.RELATIONSHIP_TYPE,  
  m.IMO_NUMBER  
FROM xwrl_party_alias a,      
  xwrl_party_master m      
WHERE m.id = a.master_id  
and a.id = :pId]]></SQLQuery>
