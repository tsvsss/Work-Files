<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="AliasNameDuplicateCheckView"
  Version="12.1.3.10.47"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="com.rmi.manualtradecompliance.adfbc.views.AliasNameDuplicateCheckView_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="p_primary_master_id"
    Kind="where"
    Type="java.lang.Integer"/>
  <Variable
    Name="pfullname"
    Kind="where"
    Type="java.lang.String"/>
  <SQLQuery><![CDATA[SELECT 
    COUNT(1) name_count 
FROM 
    xwrl_party_alias 
WHERE 
    end_date IS NULL 
    AND master_id = :p_primary_master_id 
    AND upper(full_name) = upper(:pfullname)]]></SQLQuery>
  <ViewAttribute
    Name="NameCount"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.lang.Integer"
    ColumnType="NUMBER"
    AliasName="NAME_COUNT"
    Expression="NAME_COUNT"
    SQLType="INTEGER"/>
  <AttrArray Name="KeyAttributes">
    <Item Value="NameCount"/>
  </AttrArray>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="com.rmi.manualtradecompliance.adfbc.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
