<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="RecordLock"
  Version="12.1.3.10.47"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="com.rmi.manualtradecompliance.adfbc.views.RecordLock_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="pUserId"
    Kind="where"
    Type="java.lang.Integer"/>
  <SQLQuery><![CDATA[SELECT ra.action_id   
    FROM fnd_lookup_values  fa, 
        fnd_user           fu, 
        xwrl.xwrl_role_actions ra 
    WHERE fu.user_name = fa.lookup_code 
    and fa.lookup_type = ra.role 
    AND fa.lookup_type in ('WORLD_CHECK_APPROVERS','CORPORATE_REVIEWERS')         
    AND fa.end_date_active is null 
    AND ra.action_id = 'UNLOCK_RECORD' 
    AND ra.enabled_flag = 'Y' 
    and fu.user_id = :pUserId]]></SQLQuery>
  <ViewAttribute
    Name="ActionId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="255"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="ACTION_ID"
    Expression="ACTION_ID"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="50"/>
    </DesignTime>
  </ViewAttribute>
  <AttrArray Name="KeyAttributes">
    <Item Value="ActionId"/>
  </AttrArray>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="com.rmi.manualtradecompliance.adfbc.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
