  <SQLQuery><![CDATA[SELECT WcContentEO.WC_CONTENT_ID,       
       WcContentEO.WC_MATCHES_ID,       
       WcContentEO.XML_RESPONSE,       
       WcContentEO.CREATED_BY,       
       WcContentEO.CREATION_DATE,       
       WcContentEO.LAST_UPDATED_BY,       
       WcContentEO.LAST_UPDATE_DATE,       
       WcContentEO.LAST_UPDATE_LOGIN,       
       WcContentEO.MATCHENTITYIDENTIFIER,       
       WcContentEO.MATCHIDENTIFIER,       
       WcContentEO.MATCHSCORE,       
       WcContentEO.MATCHSTATUS,       
       WcContentEO.MATCHTYPE,       
       WcContentEO.SURNAME,       
       WcContentEO.GIVEN_NAME,       
       WcContentEO.DATE_OF_BIRTH,       
       WcContentEO.DATE_OF_DEATH,       
       WcContentEO.SEX,       
       WcContentEO.STATUS,       
       WcContentEO.PASSPORT_NUMBER,       
       WcContentEO.NOTES,       
       WcContentEO.USER_CLEARABLE,       
       WcContentEO.WC_SCREENING_REQUEST_ID,       
       WcContentEO.NM_NAME,       
       WcContentEO.NM_DOB_AGE,       
       WcContentEO.NM_SEX,       
       WcContentEO.NM_DEAD,       
       WcContentEO.NM_VISUAL,       
       WcContentEO.NM_FATHER_NAME,       
       WcContentEO.NM_IMO_NUMBER,       
       WcContentEO.NM_CNNM, 
       (CASE 
          WHEN NVL(WcContentEO.MATCHSTATUS, 'INITIAL') = 'INITIAL' THEN 'A' 
          WHEN WcContentEO.MATCHSTATUS = 'POSSIBLE' THEN 'B' 
          WHEN WcContentEO.MATCHSTATUS = 'POSITIVE' THEN 'C' 
          WHEN WcContentEO.MATCHSTATUS = 'NEGATIVE' THEN 'D' 
          WHEN WcContentEO.MATCHSTATUS = 'REVIEW' THEN 'E' 
          WHEN WcContentEO.MATCHSTATUS = 'NEW' THEN 'F' 
       END) MATCHSTATUS_ORDER_BY 
FROM  VSSL.WC_CONTENT WcContentEO     
WHERE  WcContentEO.WC_MATCHES_ID=(SELECT WC_MATCHES_ID FROM WC_MATCHES WHERE WC_SCREENING_REQUEST_ID=:pWcScreeningBV)   
ORDER BY MATCHSTATUS_ORDER_BY DESC]]></SQLQuery>
