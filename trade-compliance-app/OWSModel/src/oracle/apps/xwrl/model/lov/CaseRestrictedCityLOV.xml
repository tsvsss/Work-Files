    <!--T20200707.0020 - RE: Ukraine/Russia City list (IRI): Order by clause updated with Sort_order column -->
  <SQLQuery><![CDATA[SELECT w.wc_city_list_id       
,w.city_name       
,w.subdivision       
,w.country_code       
,w.status       
,rs.restricted_country  restricted_country    
,rs.iso_alpha2_code ctry_code 
--T20220228.0033 - Enhance WC_CITY_LIST for Start Date/End Date
FROM wc_city_list_v w       
, (SELECT iso_alpha2_code,COUNTRY_CODE ,COUNTRY_NAME AS restricted_country       
FROM SICD_COUNTRIES)    rs       
where w.country_code = rs.COUNTRY_CODE  
--and w.status <> 'TC_OK'        
order by sort_order,w.country_code, w.city_name  
  
]]></SQLQuery>
