<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="CaseDocumentsLOV"
  Version="12.1.3.10.8"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full"
  FetchMode="FETCH_AS_NEEDED">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="oracle.apps.xwrl.model.lov.CaseDocumentsLOV_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="pDept"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="pPriority"
    Kind="where"
    Type="java.lang.String"/>
  <SQLQuery><![CDATA[SELECT CODE AS OptionKey,DESCRIPTION AS OptionValue        
FROM XWRL_DOCUMENT_REFERENCE        
WHERE DEPARTMENT = :pDept  
and PRIORITY_LEVEL = :pPriority
and display_flag='Y'
ORDER BY SORT_KEY ASC]]></SQLQuery>
  <ViewAttribute
    Name="Optionkey"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="255"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="OPTIONKEY"
    Expression="OPTIONKEY"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="500"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="Optionvalue"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="500"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="OPTIONVALUE"
    Expression="OPTIONVALUE"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="500"/>
    </DesignTime>
  </ViewAttribute>
  <AttrArray Name="KeyAttributes">
    <Item Value="Optionkey"/>
  </AttrArray>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="oracle.apps.xwrl.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
