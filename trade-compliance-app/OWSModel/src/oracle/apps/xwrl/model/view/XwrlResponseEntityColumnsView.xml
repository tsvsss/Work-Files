  <!-- T20200717.0031 - OWS TC - Display Summary column in Alerts table (IRI): Summary column added -->
  <SQLQuery><![CDATA[SELECT XwrlResponseEntityColumns.LISTENTITYNAME,         
       XwrlResponseEntityColumns.LISTPRIMARYNAME,         
       XwrlResponseEntityColumns.LISTNAMETYPE,         
       XwrlResponseEntityColumns.LISTCOUNTRY,         
       XwrlResponseEntityColumns.LISTCITY,         
       XwrlResponseEntityColumns.LISTOPERATINGCOUNTRIES,         
       XwrlResponseEntityColumns.LISTREGISTRATIONCOUNTRIES,         
       XwrlResponseEntityColumns.EXTERNALSOURCES,         
       XwrlResponseEntityColumns.CACHEDEXTSOURCES,         
       XwrlResponseEntityColumns.CATEGORY,         
       XwrlResponseEntityColumns.DNVESSELINDICATOR,         
       XwrlResponseEntityColumns.DNVESSELINFO,         
       XwrlResponseEntityColumns.DNADDEDDATE,         
       XwrlResponseEntityColumns.DNLASTUPDATEDDATE,         
       XwrlResponseEntityColumns.MATCHSCORE,         
       XwrlResponseEntityColumns.ALERTID,         
       XwrlResponseEntityColumns.CASEKEY,         
       XwrlResponseEntityColumns.CREATED_BY,         
       XwrlResponseEntityColumns.CREATION_DATE,         
       XwrlResponseEntityColumns.DNADDRESS,         
       XwrlResponseEntityColumns.DNADDRESSCOUNTRYCODE,         
       XwrlResponseEntityColumns.DNALLCOUNTRYCODES,         
       XwrlResponseEntityColumns.DNCITY,         
       XwrlResponseEntityColumns.DNENTITYNAME,         
       XwrlResponseEntityColumns.DNINACTIVEFLAG,         
       XwrlResponseEntityColumns.DNINACTIVESINCEDATE,         
       XwrlResponseEntityColumns.DNNAMETYPE,         
       XwrlResponseEntityColumns.DNNAMEQUALITY,         
       XwrlResponseEntityColumns.DNOPERATINGCOUNTRYCODES,         
       XwrlResponseEntityColumns.DNORIGINALENTITYNAME,         
       XwrlResponseEntityColumns.DNPEPCLASSIFICATION,         
       XwrlResponseEntityColumns.DNPOSTALCODE,         
       XwrlResponseEntityColumns.DNPRIMARYNAME,         
       XwrlResponseEntityColumns.DNREGISTRATIONCOUNTRYCODE,         
       XwrlResponseEntityColumns.DNREGISTRATIONNUMBER,         
       XwrlResponseEntityColumns.DNSTATE,         
       XwrlResponseEntityColumns.ID,         
       XwrlResponseEntityColumns.LAST_UPDATE_DATE,         
       XwrlResponseEntityColumns.LAST_UPDATE_LOGIN,         
       XwrlResponseEntityColumns.LAST_UPDATED_BY,         
       XwrlResponseEntityColumns.LISTID,         
       XwrlResponseEntityColumns.LISTKEY,         
       XwrlResponseEntityColumns.LISTORIGINALSCRIPTNAME,         
       XwrlResponseEntityColumns.LISTRECORDORIGIN,         
       XwrlResponseEntityColumns.LISTRECORDTYPE,         
       XwrlResponseEntityColumns.LISTSUBKEY,         
       XwrlResponseEntityColumns.MATCHRULE,         
       XwrlResponseEntityColumns.REC,         
       XwrlResponseEntityColumns.REQUEST_ID,         
       XwrlResponseEntityColumns.RISKSCORE,         
       XwrlResponseEntityColumns.RISKSCOREPEP,         
       XwrlResponseEntityColumns.X_STATE,    
       XwrlResponseEntityColumns.LEGAL_REVIEW,  
       nvl(XwrlResponseEntityColumns.LEGAL_REVIEW,'N') LEGAL_REVIEW_ATTR, 
       XwrlResponseEntityColumns.corporate_review, 
       (select value from xwrl_response_rows where request_id=XwrlResponseEntityColumns.request_id and XwrlResponseEntityColumns.rec=rec_row and display='Y' and label='Summary') Summary,  
       substr(XwrlResponseEntityColumns.x_state,7,length(XwrlResponseEntityColumns.x_state)) stateAttr    
       ,RMI_OWS_COMMON_UTIL.GET_OWS_ALERT_STATE(XwrlResponseEntityColumns.ALERTID) owsAlertState    
       , decode(UTL_MATCH.edit_distance(XwrlResponseEntityColumns.X_STATE,XWRL_OWS_UTILS.CHANGEOWSSTATE(RMI_OWS_COMMON_UTIL.GET_OWS_ALERT_STATE(XwrlResponseEntityColumns.ALERTID))),0,'Y','N') as sameState   
	   ,DECODE(XwrlResponseEntityColumns.LEGAL_REVIEW,'Y','Legal Checked','Legal Unchecked') LEGAL_CHECKED  
	   ,CASE  
  WHEN substr(XwrlResponseEntityColumns.x_state,7,length(XwrlResponseEntityColumns.x_state)) = 'Open'  
  THEN   
       substr(XwrlResponseEntityColumns.x_state,7,length(XwrlResponseEntityColumns.x_state))   
       ||' '||   
       DECODE(XwrlResponseEntityColumns.LEGAL_REVIEW,'Y','Legal Checked','Legal Unchecked')  
   WHEN substr(XwrlResponseEntityColumns.x_state,7,length(XwrlResponseEntityColumns.x_state)) = 'False Positive'  
  THEN   
       substr(XwrlResponseEntityColumns.x_state,7,length(XwrlResponseEntityColumns.x_state))   
   WHEN substr(XwrlResponseEntityColumns.x_state,7,length(XwrlResponseEntityColumns.x_state)) = 'Possible'  
  THEN   
       substr(XwrlResponseEntityColumns.x_state,7,length(XwrlResponseEntityColumns.x_state))   
    WHEN substr(XwrlResponseEntityColumns.x_state,7,length(XwrlResponseEntityColumns.x_state)) = 'Positive'  
  THEN   
       substr(XwrlResponseEntityColumns.x_state,7,length(XwrlResponseEntityColumns.x_state))       
END as STATE_LEGAL  
FROM  XWRL_RESPONSE_ENTITY_COLUMNS XwrlResponseEntityColumns      
order by decode ((CASE  
  WHEN substr(XwrlResponseEntityColumns.x_state,7,length(XwrlResponseEntityColumns.x_state)) = 'Open'  
  THEN   
       substr(XwrlResponseEntityColumns.x_state,7,length(XwrlResponseEntityColumns.x_state))   
       ||' '||   
       DECODE(XwrlResponseEntityColumns.LEGAL_REVIEW,'Y','Legal Checked','Legal Unchecked')  
   WHEN substr(XwrlResponseEntityColumns.x_state,7,length(XwrlResponseEntityColumns.x_state)) = 'False Positive'  
  THEN   
       substr(XwrlResponseEntityColumns.x_state,7,length(XwrlResponseEntityColumns.x_state))   
   WHEN substr(XwrlResponseEntityColumns.x_state,7,length(XwrlResponseEntityColumns.x_state)) = 'Possible'  
  THEN   
       substr(XwrlResponseEntityColumns.x_state,7,length(XwrlResponseEntityColumns.x_state))   
    WHEN substr(XwrlResponseEntityColumns.x_state,7,length(XwrlResponseEntityColumns.x_state)) = 'Positive'  
  THEN   
       substr(XwrlResponseEntityColumns.x_state,7,length(XwrlResponseEntityColumns.x_state))       
END)     
                 ,'Open Legal Unchecked',1    
                 ,'Open Legal Checked',2   
                 ,'Positive',4     
                 ,'Possible',3     
                 ,'False Positive',5) asc, rec desc, XwrlResponseEntityColumns.REC desc]]></SQLQuery>
