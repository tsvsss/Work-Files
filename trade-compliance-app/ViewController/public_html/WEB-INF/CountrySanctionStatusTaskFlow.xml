<?xml version="1.0" encoding="UTF-8" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="CountrySanctionStatusTaskFlow">
    <default-activity>execSanction</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <managed-bean id="__1">
      <managed-bean-name>CountrySanctionStatus</managed-bean-name>
      <managed-bean-class>oracle.apps.xwrl.beans.backing.CountrySanctionStatus</managed-bean-class>
      <managed-bean-scope>backingBean</managed-bean-scope>
    </managed-bean>
    <view id="CountrySanctionStatus">
      <page>/Prototype/CountrySanctionStatus.jsff</page>
    </view>
    <method-call id="execSanction">
      <method>#{bindings.execSanction.execute}</method>
      <outcome>
        <fixed-outcome>execSanction</fixed-outcome>
      </outcome>
    </method-call>
    <control-flow-rule id="__2">
      <from-activity-id>execSanction</from-activity-id>
      <control-flow-case id="__3">
        <from-outcome>execSanction</from-outcome>
        <to-activity-id>CountrySanctionStatus</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
