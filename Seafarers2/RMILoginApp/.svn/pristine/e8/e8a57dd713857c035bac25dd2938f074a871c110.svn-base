<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="UserSubMenuView"
  Version="12.1.3.10.8"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="com.rmi.login.adfbc.views.readonly.UserSubMenuView_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <SQLQuery><![CDATA[SELECT DISTINCT    
    RMI_ROLE_MENU_ITEM.MENU_ITEM_TITLE,    
    RMI_ROLE_MENU.MENU_TITLE,    
    RMI_ROLE_MENU_ITEM.ACTIVE_FLAG,    
    RMI_ROLE_MENU_ITEM.DISPLAY_ORDER,    
    RMI_ROLE_MENU_ITEM.TASKFLOW_ID,    
    RMI_ROLE_MENU_ITEM.ROLE_ID,    
    RMI_USER_ROLES.USER_ID    
FROM    
    RMI_SEAFARER.RMI_USERS_ROLES RMI_USER_ROLES,    
    RMI_SEAFARER.RMI_ROLES RMI_ROLES,    
    RMI_SEAFARER.RMI_ROLE_MENU RMI_ROLE_MENU,    
    RMI_SEAFARER.RMI_ROLE_MENU_ITEM RMI_ROLE_MENU_ITEM    
WHERE    
    RMI_USER_ROLES.ROLE_ID = RMI_ROLES.ROLE_ID AND RMI_ROLES.ROLE_ID = RMI_ROLE_MENU.ROLE_ID AND RMI_ROLE_MENU.ROLE_ID = RMI_ROLE_MENU_ITEM.ROLE_ID AND RMI_ROLE_MENU.MAIN_MENU_ID = RMI_ROLE_MENU_ITEM.MAIN_MENU_ID AND RMI_USER_ROLES.ACTIVE_FLAG = 'Y' AND RMI_ROLES.ACTIVE_FLAG = 'Y' AND RMI_ROLE_MENU.ACTIVE_FLAG = 'Y' AND RMI_ROLE_MENU_ITEM.ACTIVE_FLAG = 'Y'    
ORDER BY    
    RMI_ROLE_MENU_ITEM.DISPLAY_ORDER ASC]]></SQLQuery>
  <ViewAttribute
    Name="MenuItemTitle"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="255"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="MENU_ITEM_TITLE"
    Expression="MENU_ITEM_TITLE"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="100"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="MenuTitle"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="30"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="MENU_TITLE"
    Expression="MENU_TITLE"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="30"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="ActiveFlag"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="1"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="ACTIVE_FLAG"
    Expression="ACTIVE_FLAG"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="1"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="DisplayOrder"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.math.BigDecimal"
    ColumnType="NUMBER"
    AliasName="DISPLAY_ORDER"
    Expression="DISPLAY_ORDER"
    SQLType="NUMERIC"/>
  <ViewAttribute
    Name="TaskflowId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="100"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="TASKFLOW_ID"
    Expression="TASKFLOW_ID"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="100"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="RoleId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.math.BigDecimal"
    ColumnType="NUMBER"
    AliasName="ROLE_ID"
    Expression="ROLE_ID"
    SQLType="NUMERIC"/>
  <ViewAttribute
    Name="UserId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.math.BigDecimal"
    ColumnType="NUMBER"
    AliasName="USER_ID"
    Expression="USER_ID"
    SQLType="NUMERIC"/>
  <AttrArray Name="KeyAttributes">
    <Item Value="MenuItemTitle"/>
    <Item Value="MenuTitle"/>
    <Item Value="TaskflowId"/>
    <Item Value="UserId"/>
  </AttrArray>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
