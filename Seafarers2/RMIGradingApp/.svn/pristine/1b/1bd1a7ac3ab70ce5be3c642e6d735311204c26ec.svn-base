  </ViewAttribute>
  <ViewAttribute
    Name="Subdivision"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="100"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="SUBDIVISION"
    Expression="SUBDIVISION"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="100"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="WcCityListId"
    IsUpdateable="false"
    IsPersistent="false"
