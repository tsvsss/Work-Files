<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="PreviousIdCardView"
  Version="12.1.3.10.8"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="com.rmi.tradecompapproval.adfbc.views.lovs.PreviousIdCardView_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="pSeafarerId"
    Kind="where"
    Type="java.lang.Integer">
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYHINT
          Value="Hide"/>
      </SchemaBasedProperties>
    </Properties>
  </Variable>
  <Variable
    Name="pDocumentId"
    Kind="where"
    Type="java.lang.Integer">
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYHINT
          Value="Hide"/>
      </SchemaBasedProperties>
    </Properties>
  </Variable>
  <SQLQuery><![CDATA[select sd.book_number,      
to_char(sd.expiration_date,'dd-MON-yyyy') expiration_date,      
sd.status,      
sd.document_id        
from sicd_documents sd        
where sd.seafarer_id = :pSeafarerId        
AND sd.document_id = nvl(:pDocumentId, sd.document_id)        
and sd.DOCUMENT_TYPE= 'BOOK'        
and sd.book_type = 'IDCARD'  
order by sd.expiration_date desc]]></SQLQuery>
