  <SQLQuery><![CDATA[SELECT                             
ROWNUM SL_NO,                             
QUERY_ID,                             
WEIGHT,                             
SEAFARER_ID,                             
LAST_NAME,                             
FIRST_NAME,                             
BIRTH_DATE,     
TO_CHAR(BIRTH_DATE,'yyyy/MM/dd') BIRTH_DATE_CHAR,    
BIRTH_PLACE,                             
CITIZENSHIP,                             
NATIONALITY,                             
HAIR_COLOR,                             
EYE_COLOR,                             
GENDER,                             
HEIGHT_FT,                             
HEIGHT_IN,                             
WEIGHT_LB,                             
DISTINGUISHING_MARKS,                            
NOTES,                           
iri_html.get_url ('SICD') ||                     
(SELECT url                     
FROM iri_edocs                     
WHERE IDENTIFIER         = TO_CHAR (seafarer_id)                     
AND marked_for_deletion IS NULL                     
AND category_id          = 450                     
AND SEQUENCE             =                     
  (SELECT MAX (SEQUENCE)                     
  FROM iri_edocs                     
  WHERE IDENTIFIER         = TO_CHAR (seafarer_id)                     
  AND marked_for_deletion IS NULL                     
  AND category_id          = 450                     
  )                     
AND rownum<=1                     
) PHOTO                           
FROM APPS.RMI_SEAFARER_SEARCH_V                                
WHERE QUERY_ID = :pQueryId                   
ORDER BY WEIGHT DESC]]></SQLQuery>
