       SicdSeafarerEO.ATTRIBUTE7,                      
       SicdSeafarerEO.ATTRIBUTE8,                      
       SicdSeafarerEO.ATTRIBUTE9,                      
       SicdSeafarerEO.ATTRIBUTE_CATEGORY,                      
       SicdSeafarerEO.BIRTH_DATE,                      
       SicdSeafarerEO.BIRTH_PLACE,                      
       SicdSeafarerEO.CREATED_BY,                      
       SicdSeafarerEO.CREATION_DATE,                      
       SicdSeafarerEO.DATA_VERIFIED_BY,                      
       SicdSeafarerEO.DATA_VERIFIED_DATE,                      
       SicdSeafarerEO.DISTINGUISHING_MARKS,                      
       SicdSeafarerEO.EYE_COLOR,                      
       SicdSeafarerEO.FIRST_NAME,                      
       SicdSeafarerEO.GENDER,                      
       SicdSeafarerEO.HAIR_COLOR,                      
       SicdSeafarerEO.HEIGHT_FT,                      
       SicdSeafarerEO.HEIGHT_IN,                      
       SicdSeafarerEO.LAST_NAME,                      
       SicdSeafarerEO.LAST_UPDATE_DATE,                      
       SicdSeafarerEO.LAST_UPDATE_LOGIN,                      
       SicdSeafarerEO.LAST_UPDATED_BY,                      
       SicdSeafarerEO.MIDDLE_INITIAL,                      
       SicdSeafarerEO.NATIONALITY,                      
       SicdSeafarerEO.NOTES,                      
       SicdSeafarerEO.SEAFARER_ID,                      
       SicdSeafarerEO.STATUS,                      
       SicdSeafarerEO.WEIGHT_LB,                      
--       (SELECT disk_path                         
--        FROM iri_edocs                         
--        WHERE IDENTIFIER         = TO_CHAR (seafarer_id)                         
--        AND marked_for_deletion IS NULL                         
--        AND category_id          = 450                         
--        AND SEQUENCE             =                         
--          (SELECT MAX (SEQUENCE)                         
--          FROM iri_edocs                         
--          WHERE IDENTIFIER         = TO_CHAR (seafarer_id)                         
--          AND marked_for_deletion IS NULL                         
--          AND category_id          = 450                         
--          )                         
--        AND rownum<=1                         
--       ) PHOTO,   
--       sicd_img.get_photo_file_path(seafarer_id) photo_path,  
       iri_html.get_base_path('SICD_PHOTOS')||seafarer_id|| '.jpeg' PHOTO  
FROM  SICD_SEAFARERS SicdSeafarerEO                     
WHERE (SEAFARER_ID = :pSeafarerId  
or UPPER(NVL(FIRST_NAME,'NULL')) = UPPER(:pFirstName)                 
or UPPER(NVL(LAST_NAME,'NULL')) = UPPER(:pLastName)                 
or UPPER(NVL(NATIONALITY,'NULL')) = UPPER(:pNationality)                      
or NVL(BIRTH_DATE,sysdate) = :pBirthDate)  
  
/*or UPPER(NVL(FIRST_NAME,'NULL')) = UPPER(NVL(:pFirstName, NVL(FIRST_NAME,'NULL')))                 
--or UPPER(NVL(LAST_NAME,'NULL')) = UPPER(NVL(:pLastName, NVL(LAST_NAME,'NULL')))                 
--or UPPER(NVL(NATIONALITY,'NULL')) = UPPER(NVL(:pNationality, NVL(NATIONALITY,'NULL')))                      
--or NVL(BIRTH_DATE,sysdate) = NVL(:pBirthDate, NVL(BIRTH_DATE,sysdate)) */  
  
and ATTRIBUTE15 IS NULL ORDER BY LAST_NAME,FIRST_NAME]]></SQLQuery>
