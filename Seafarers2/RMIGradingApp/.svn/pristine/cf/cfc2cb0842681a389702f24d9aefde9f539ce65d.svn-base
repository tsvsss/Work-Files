FROM                      
OE_ORDER_HEADERS_ALL                      
WHERE   
SOLD_TO_ORG_ID = NVL(:pCustomer, SOLD_TO_ORG_ID)                      
AND NVL(CUST_PO_NUMBER,'NULL') = NVL(:pCustomerPo, NVL(CUST_PO_NUMBER,'NULL'))                      
AND ORDER_NUMBER = NVL(:pOrderNumber, ORDER_NUMBER)     
AND HEADER_ID = NVL(:pHeaderId, HEADER_ID)      
AND ORDER_TYPE_ID = 1003        
ORDER BY CREATION_DATE DESC]]></SQLQuery>
