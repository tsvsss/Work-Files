      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__4">
      <from-activity-id>EditDocumentsPage</from-activity-id>
      <control-flow-case id="__5">
        <from-outcome>back</from-outcome>
        <to-activity-id>taskFlowReturn1</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__9">
        <from-outcome>tradeCompliance</from-outcome>
        <to-activity-id>vettingTF</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
