    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="ItemType"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="8"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="ITEM_TYPE"
