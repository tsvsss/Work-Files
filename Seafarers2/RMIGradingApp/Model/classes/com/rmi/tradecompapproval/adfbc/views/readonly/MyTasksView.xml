<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="MyTasksView"
  Version="12.1.3.10.8"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="com.rmi.tradecompapproval.adfbc.views.readonly.MyTasksView_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="pUserId"
    Kind="where"
    Type="java.lang.Integer"/>
  <SQLQuery><![CDATA[select wf.BATCH_ID,bt.batch_status,bt.batch_date,bt.batch_name,              
(select count(esi_id) from RMI_SEAFARER.RMI_SEAFARERS_IFACE               
where esi_batch_id = bt.esi_batch_id) no_of_seafarer,              
(SELECT jrs.name              
            FROM RMI_SEAFARER.RMI_USER ua,jtf_rs_salesreps jrs              
           WHERE ua.user_id =  bt.created_by             
             AND ua.default_processing_office = to_char(jrs.salesrep_id)) region ,           
             (select count(*) from RMI_SEAFARER.RMI_SEAFARERS_IFACE sea ,             
RMI_SEAFARER.RMI_SEAFARER_DOCS_IFACE docs              
where docs.esi_id = sea.esi_id and             
sea.esi_batch_id = bt.esi_batch_id and             
docs.cra_required = 'Y') cra_count           
from               
   RMI_SEAFARER.RMI_WORKFLOW wf              
  ,RMI_SEAFARER.RMI_BATCH_IFACE bt               
where              
wf.BATCH_ID = bt.esi_batch_id              
and wf.assigned_to = :pUserId              
and wf.LAST_UPDATED_ROW='Y'          
order by bt.batch_date desc]]></SQLQuery>
  <ViewAttribute
    Name="BatchStatus"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="100"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="BATCH_STATUS"
    Expression="BATCH_STATUS"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="100"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="BatchDate"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="oracle.jbo.domain.Date"
    ColumnType="DATE"
    AliasName="BATCH_DATE"
    Expression="BATCH_DATE"
    SQLType="DATE"/>
  <ViewAttribute
    Name="BatchName"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="30"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="BATCH_NAME"
    Expression="BATCH_NAME"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="30"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="NoOfSeafarer"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.lang.Integer"
    ColumnType="NUMBER"
    AliasName="NO_OF_SEAFARER"
    Expression="NO_OF_SEAFARER"
    SQLType="INTEGER"/>
  <ViewAttribute
    Name="Region"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="240"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="REGION"
    Expression="REGION"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="240"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="CraCount"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.lang.Integer"
    ColumnType="NUMBER"
    AliasName="CRA_COUNT"
    Expression="CRA_COUNT"
    SQLType="INTEGER"/>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="com.rmi.tradecompapproval.adfbc.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
