<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="OrderLinesHoldFlagView"
  Version="12.1.3.10.47"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  FetchMode="FETCH_AS_NEEDED"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="com.rmi.tradecompapproval.adfbc.views.readonly.OrderLinesHoldFlagView_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="pHeaderId"
    Kind="where"
    Type="java.lang.Integer"/>
  <Variable
    Name="pLineId"
    Kind="where"
    Type="java.lang.Integer"/>
  <SQLQuery><![CDATA[select      
NVL ((SELECT 'Y'      
               FROM oe_order_holds_all d      
              WHERE d.header_id = :pHeaderId      
                AND d.line_id = :pLineId      
                AND d.hold_release_id IS NULL      
                AND ROWNUM <= 1),      
            'N'      
           ) order_hold_flag      
from dual]]></SQLQuery>
  <ViewAttribute
    Name="OrderHoldFlag"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="255"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="ORDER_HOLD_FLAG"
    Expression="ORDER_HOLD_FLAG"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="1"/>
    </DesignTime>
  </ViewAttribute>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="com.rmi.tradecompapproval.adfbc.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
