<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="GetEmailByContactIdView"
  Version="12.1.3.10.47"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="com.rmi.applicationEntry.adfbc.views.readOnlyVO.GetEmailByContactIdView_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="pContactPointId"
    Kind="where"
    Type="java.lang.Integer"/>
  <SQLQuery><![CDATA[SELECT contact_point_id cust_contact_point_id,          
                  email_address         
            FROM hz_contact_points         
           WHERE owner_table_name = 'HZ_PARTIES'         
             AND status = 'A'         
             AND contact_point_type = 'EMAIL'         
             AND contact_point_purpose in ('SICD_CRA_EMAIL', 'SICD_INVOICE_EMAIL')    
        and contact_point_id = :pContactPointId]]></SQLQuery>
  <ViewAttribute
    Name="CustContactPointId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.lang.Integer"
    ColumnType="LONGINT"
    AliasName="CUST_CONTACT_POINT_ID"
    Expression="CUST_CONTACT_POINT_ID"
    SQLType="INTEGER"/>
  <ViewAttribute
    Name="EmailAddress"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="2000"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="EMAIL_ADDRESS"
    Expression="EMAIL_ADDRESS"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="2000"/>
    </DesignTime>
  </ViewAttribute>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="com.rmi.applicationEntry.adfbc.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
