  <SQLQuery><![CDATA[select wf.esi_batch_id,bt.batch_status,trunc(nvl(bt.batch_submitted_date,bt.batch_date)) batch_date,bt.batch_name,                                   
(select count(esi_id) from rmi_seafarer.rmi_seafarers_iface                                    
where esi_batch_id = bt.esi_batch_id) no_of_seafarer,                                   
jrs.name region ,                                   
jrs.salesrep_id                        
            region_id ,                           
             (select order_number from OE_ORDER_HEADERS_ALL                       
where OE_ORDER_HEADERS_ALL.header_id = bt.oe_header_id) work_order_no,                
(select user_type from rmi_seafarer.rmi_user where user_id = bt.created_by) order_type,               
nvl(bt.expedited_processing,'N') expedited_processing,              
(select count(*) from rmi_seafarer.rmi_seafarers_iface sea ,                  
rmi_seafarer.rmi_seafarer_docs_iface docs                   
where docs.esi_id = sea.esi_id and                  
sea.esi_batch_id = bt.esi_batch_id and                  
docs.cra_required = 'Y' and nvl(docs.cra_approved_flag,'NA') != 'N') cra_count,    
(select count(*) from rmi_seafarer.rmi_seafarers_iface sea ,                     
rmi_seafarer.rmi_seafarer_docs_iface docs                      
where docs.esi_id = sea.esi_id and                     
sea.esi_batch_id = bt.esi_batch_id and                     
docs.cra_required = 'Y'                  
and docs.cra_sent_date is null and nvl(docs.cra_approved_flag,'NA') != 'N') cra_sent_count    
from                                    
   rmi_seafarer.rmi_workflow wf                                   
  ,rmi_seafarer.rmi_batch_iface bt ,                     
  rmi_seafarer.rmi_user ua,jtf_rs_salesreps jrs                     
where                                   
wf.esi_batch_id = bt.esi_batch_id                                   
and wf.assigned_to is null                        
and wf.batch_status = 'Pending_MTC'                     
and wf.LAST_UPDATED_ROW='Y'                       
and bt.created_by = ua.user_id(+)                       
AND ua.default_processing_office = jrs.salesrep_id(+)                     
and nvl(bt.batch_assigned_to_region,jrs.salesrep_id) = (select default_processing_office from rmi_seafarer.rmi_user where                     
user_id = :pUserId)          
and bt.batch_status = 'Resubmitted'         
order by DECODE(expedited_processing,'Y',1,0) desc,cra_sent_count desc, work_order_no asc]]></SQLQuery>
