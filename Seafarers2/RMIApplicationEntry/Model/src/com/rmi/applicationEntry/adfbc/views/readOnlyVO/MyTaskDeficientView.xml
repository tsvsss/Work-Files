  <Variable
    Name="pUserId"
    Kind="where"
    Type="java.lang.Integer"/>
  <SQLQuery><![CDATA[select wf.esi_batch_id,bt.batch_status,trunc(nvl(bt.batch_submitted_date,bt.batch_date)) batch_date,bt.batch_name,                          
(select count(esi_id) from rmi_seafarer.rmi_seafarers_iface                           
where esi_batch_id = bt.esi_batch_id) no_of_seafarer,                          
(SELECT jrs.name                          
            FROM rmi_seafarer.rmi_user ua,jtf_rs_salesreps jrs                          
           WHERE ua.user_id =  bt.created_by                         
             AND ua.default_processing_office = to_char(jrs.salesrep_id)) region ,                       
             (select count(*) from rmi_seafarer.rmi_seafarers_iface sea ,                          
rmi_seafarer.rmi_seafarer_docs_iface docs                           
where docs.esi_id = sea.esi_id and                          
sea.esi_batch_id = bt.esi_batch_id and                          
docs.cra_required = 'Y'          
and nvl(docs.cra_approved_flag,'NA') != 'N'  
and nvl(docs.grading_status,'Pending') not in ('Cancel','Rejected')) cra_count,                     
(select count(*) from rmi_seafarer.rmi_seafarers_iface sea ,                           
rmi_seafarer.rmi_seafarer_docs_iface docs                            
where docs.esi_id = sea.esi_id and                           
sea.esi_batch_id = bt.esi_batch_id and                           
docs.cra_required = 'Y'                        
and docs.cra_sent_date is null and nvl(docs.cra_approved_flag,'NA') != 'N'  
and nvl(docs.grading_status,'Pending') not in ('Cancel','Rejected')) cra_sent_count,                    
(select user_type from rmi_seafarer.rmi_user                     
where user_id = bt.created_by) order_source,                  
(select order_number from OE_ORDER_HEADERS_ALL                             
where OE_ORDER_HEADERS_ALL.header_id = bt.oe_header_id) work_order_no,                 
wf.batch_status wf_status,                
bt.oe_header_id,     
decode(bt.expedited_processing,'N','false','Y','true','false') expedited_processing_mytask          
from                           
   rmi_seafarer.rmi_workflow wf                          
  ,rmi_seafarer.rmi_batch_iface bt                           
where                          
wf.esi_batch_id = bt.esi_batch_id                          
and wf.assigned_to = :pUserId                          
and wf.LAST_UPDATED_ROW='Y'                    
and wf.batch_status = 'Deficient'                                                  
order by DECODE(expedited_processing,'Y',1,0) desc,decode(cra_sent_count,0,0,1) desc,    
work_order_no asc, batch_submitted_date asc,esi_batch_id asc]]></SQLQuery>
