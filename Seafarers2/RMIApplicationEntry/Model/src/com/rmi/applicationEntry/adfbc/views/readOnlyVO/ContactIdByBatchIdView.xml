<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="ContactIdByBatchIdView"
  Version="12.1.3.10.47"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="com.rmi.applicationEntry.adfbc.views.readOnlyVO.ContactIdByBatchIdView_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="pBatchId"
    Kind="where"
    Type="java.lang.Integer"/>
  <SQLQuery><![CDATA[select orderMap.apps_cust_contact_id from rmi_seafarer.rmi_user_order_agent_map orderMap, 
    rmi_seafarer.rmi_batch_iface batch where orderMap.user_id = batch.created_by 
    and orderMap.ordering_agent_account_id =  batch.deliver_to_cust_account_id 
    and batch.esi_batch_id = :pBatchId]]></SQLQuery>
  <ViewAttribute
    Name="AppsCustContactId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.lang.Integer"
    ColumnType="NUMBER"
    AliasName="APPS_CUST_CONTACT_ID"
    Expression="APPS_CUST_CONTACT_ID"
    SQLType="NUMERIC"/>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="com.rmi.applicationEntry.adfbc.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
