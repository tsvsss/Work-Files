    Name="EndorsementExpirationDate"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="oracle.jbo.domain.Date"
    ColumnType="DATE"
    AliasName="ENDORSEMENT_EXPIRATION_DATE"
    Expression="ENDORSEMENT_EXPIRATION_DATE"
    SQLType="DATE"/>
  <ViewAttribute
    Name="DocumentBookId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="38"
    Scale="0"
    Type="java.math.BigInteger"
    ColumnType="NUMBER"
    AliasName="DOCUMENT_BOOK_ID"
    Expression="DOCUMENT_BOOK_ID"
    SQLType="NUMERIC"/>
  <ViewAttribute
    Name="DocChargeable"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="1"
    Type="java.lang.String"
