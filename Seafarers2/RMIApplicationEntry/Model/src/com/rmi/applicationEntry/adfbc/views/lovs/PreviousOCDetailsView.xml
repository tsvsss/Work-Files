<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="PreviousOCDetailsView"
  Version="12.1.3.10.47"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="com.rmi.applicationEntry.adfbc.views.lovs.PreviousOCDetailsView_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="pSeafarerId"
    Kind="where"
    Type="java.lang.Integer"/>
  <Variable
    Name="pGradeId"
    Kind="where"
    Type="java.lang.Integer"/>
  <SQLQuery><![CDATA[SELECT D.SEAFARER_ID, D.DOCUMENT_ID, D.GRADE_ID, D.ENDORSEMENT_NUMBER, D.ENDORSEMENT_COUNTRY, 
D.ENDORSEMENT_ISSUE_DATE, D.ENDORSEMENT_EXPIRATION_DATE, D.DOCUMENT_BOOK_ID, 
(CASE WHEN D.EXPIRATION_DATE BETWEEN SYSDATE - 364 AND SYSDATE + 364 
      THEN 'N' ELSE 'Y' END) DOC_CHARGEABLE 
FROM SICD_DOCUMENTS D 
WHERE D.DOCUMENT_TYPE = 'OC' 
AND D.STATUS = 'Active' 
AND D.SEAFARER_ID = :pSeafarerId 
AND D.GRADE_ID = :pGradeId]]></SQLQuery>
  <ViewAttribute
    Name="SeafarerId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.math.BigInteger"
    ColumnType="BIGINT"
    AliasName="SEAFARER_ID"
    Expression="SEAFARER_ID"
    SQLType="NUMERIC"/>
  <ViewAttribute
    Name="DocumentId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="38"
    Scale="0"
    Type="java.math.BigInteger"
    ColumnType="NUMBER"
    AliasName="DOCUMENT_ID"
    Expression="DOCUMENT_ID"
    SQLType="NUMERIC"/>
  <ViewAttribute
    Name="GradeId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="38"
    Scale="0"
    Type="java.math.BigInteger"
    ColumnType="NUMBER"
    AliasName="GRADE_ID"
    Expression="GRADE_ID"
    SQLType="NUMERIC"/>
  <ViewAttribute
    Name="EndorsementNumber"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="60"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="ENDORSEMENT_NUMBER"
    Expression="ENDORSEMENT_NUMBER"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="60"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="EndorsementCountry"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="4"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="ENDORSEMENT_COUNTRY"
    Expression="ENDORSEMENT_COUNTRY"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="4"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="EndorsementIssueDate"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="oracle.jbo.domain.Date"
    ColumnType="DATE"
