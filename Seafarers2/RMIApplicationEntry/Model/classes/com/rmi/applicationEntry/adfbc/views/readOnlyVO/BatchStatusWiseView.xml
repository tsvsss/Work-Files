<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="BatchStatusWiseView"
  Version="12.1.3.10.47"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="com.rmi.applicationEntry.adfbc.views.readOnlyVO.BatchStatusWiseView_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="pUserId"
    Kind="where"
    Type="java.lang.Integer"/>
  <SQLQuery><![CDATA[select count(esi_batch_id) No_of_batches, batch_status from rmi_seafarer.rmi_batch_iface   
where created_by = :pUserId  
and batch_status in ('Pending','Submitted','Returned','Grading Approved')  
group by batch_status]]></SQLQuery>
  <ViewAttribute
    Name="NoOfBatches"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.lang.Integer"
    ColumnType="NUMBER"
    AliasName="NO_OF_BATCHES"
    Expression="NO_OF_BATCHES"
    SQLType="INTEGER"/>
  <ViewAttribute
    Name="BatchStatus"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="100"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="BATCH_STATUS"
    Expression="BATCH_STATUS"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="100"/>
    </DesignTime>
  </ViewAttribute>
  <AttrArray Name="KeyAttributes">
    <Item Value="BatchStatus"/>
  </AttrArray>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="com.rmi.applicationEntry.adfbc.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
