  <SQLQuery><![CDATA[select ord.seafarer_id,ord.header_id,ord.order_number,ord.customer_number,ord.customer_name,ord.ordered_date,ord.booked_flag,ord.open_flag,ord.note,  
ord.first_name||' '||ord.last_name seafarer_name,bt.esi_batch_id,bt.batch_name,bt.purchase_order_number,bt.expedited_processing, 
(select decode(seafarer_wf_status,'Pending_TC','Pending Validation','Pending_VAL','Pending Evaluation',            
'Pending_QA','Pending QA','Pending_Shipping','Pending Shipping','Closed','Closed','Pending_ABN','Pending Assign Book Number','Rejected','Sent for correction','Unverified','Sent for correction','Pending_MTC','Pending Trade Compliance','Submitted') from rmi_seafarer.rmi_seafarers_iface where seafarer_id = ord.seafarer_id and esi_batch_id = bt.esi_batch_id) application_status 
from rmi_sicd_order_history_vw ord,rmi_seafarer.rmi_batch_iface bt where ord.seafarer_id = nvl(:pSeafarer_id,ord.seafarer_id)  
and upper(ord.last_name) = nvl(upper(:pLastName),ord.last_name)   
and trunc(ord.birth_date) = trunc(nvl(to_date(:pBirthDate),ord.birth_date))  
and (:pSeafarer_id is not null or :pLastName is not null or :pBirthDate is not null)  
and ord.header_id = bt.oe_header_id(+)]]></SQLQuery>
