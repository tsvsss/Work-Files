  Name="OrderLinesForRequestsView"
  Version="12.1.3.10.47"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="com.rmi.applicationEntry.adfbc.views.lovs.OrderLinesForRequestsView_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="pHeaderId"
    Kind="where"
    Type="java.lang.Integer"/>
  <SQLQuery><![CDATA[SELECT ooh.order_number,  
       ool.line_number || '.' || ool.shipment_number line_number,  
       ooh.header_id, ool.line_id, jrs.NAME, jrs.salesrep_id, ool.created_by,  
       fu.user_name, fu.description user_desc  
  FROM oe_order_headers_all ooh,  
       oe_order_lines_all ool,  
       jtf_rs_salesreps jrs,  
       fnd_user fu,  
       rmi_seafarer.rmi_batch_iface bti  
WHERE ooh.header_id = ool.header_id  
   AND ooh.org_id = ool.org_id  
   AND ooh.flow_status_code <> 'CANCELLED'  
   AND ool.flow_status_code <> 'CANCELLED'  
   AND jrs.salesrep_id = ooh.salesrep_id  
   AND jrs.org_id = ooh.org_id  
   AND fu.user_id = ooh.created_by  
   and ooh.header_id = bti.oe_header_id 
   and ooh.header_id = :pHeaderId]]></SQLQuery>
