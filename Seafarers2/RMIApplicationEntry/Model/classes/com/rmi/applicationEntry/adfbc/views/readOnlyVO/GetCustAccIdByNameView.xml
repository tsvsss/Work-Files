<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="GetCustAccIdByNameView"
  Version="12.1.3.10.47"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="com.rmi.applicationEntry.adfbc.views.readOnlyVO.GetCustAccIdByNameView_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="pPartyName"
    Kind="where"
    Type="java.lang.String"/>
  <SQLQuery><![CDATA[SELECT hca.cust_account_id, hp_ship.party_name         
  FROM hz_parties hp_ship, hz_cust_accounts hca         
 WHERE 1 = 1         
   AND hca.account_name = 'SICD'         
   AND hca.party_id = hp_ship.party_id     
   and hp_ship.party_name||'('||hca.account_number||')' = :pPartyName]]></SQLQuery>
  <ViewAttribute
    Name="CustAccountId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.lang.Integer"
    ColumnType="LONGINT"
    AliasName="CUST_ACCOUNT_ID"
    Expression="CUST_ACCOUNT_ID"
    SQLType="INTEGER"/>
  <ViewAttribute
    Name="PartyName"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="360"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="PARTY_NAME"
    Expression="PARTY_NAME"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="360"/>
    </DesignTime>
  </ViewAttribute>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="com.rmi.applicationEntry.adfbc.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
