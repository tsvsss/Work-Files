<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="GetContactIdByInvoiceEmailView"
  Version="12.1.3.10.47"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  FetchMode="FETCH_AS_NEEDED"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="com.rmi.applicationEntry.adfbc.views.readOnlyVO.GetContactIdByInvoiceEmailView_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="pEmail"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="pCustomerId"
    Kind="where"
    Type="java.lang.Integer"/>
  <SQLQuery><![CDATA[SELECT email_otp.contact_point_id cust_contact_point_id,              
                  email_otp.email_address   
            FROM hz_contact_points email_otp,      
            ar_contacts_v con      
           WHERE email_otp.owner_table_name = 'HZ_PARTIES'             
             AND email_otp.status = 'A'             
             AND email_otp.contact_point_type = 'EMAIL'             
             AND email_otp.contact_point_purpose in ('SICD_INVOICE_EMAIL')         
              AND email_otp.owner_table_id = con.rel_party_id         
        and email_otp.email_address = :pEmail  
        and con.customer_id = :pCustomerId]]></SQLQuery>
  <ViewAttribute
    Name="CustContactPointId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.lang.Integer"
    ColumnType="LONGINT"
    AliasName="CUST_CONTACT_POINT_ID"
    Expression="CUST_CONTACT_POINT_ID"
    SQLType="INTEGER"/>
  <ViewAttribute
    Name="EmailAddress"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="2000"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="EMAIL_ADDRESS"
    Expression="EMAIL_ADDRESS"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="2000"/>
    </DesignTime>
  </ViewAttribute>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="com.rmi.applicationEntry.adfbc.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
