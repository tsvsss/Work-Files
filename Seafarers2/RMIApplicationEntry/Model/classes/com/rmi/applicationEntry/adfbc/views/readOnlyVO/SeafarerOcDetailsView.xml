  <SQLQuery><![CDATA[select ESDI_ID,ESI_ID,         
(select grd.grade_name from sicd_grades grd where grd.grade_id = seaOC.grade_id and         
 grd.grade_type='OC'          
and grd.status = 'Active' )GRADE_ID,         
COC_IDENTIFIER,         
(select country_name from sicd_countries where country_code = coc_country)COC_COUNTRY,         
COC_ISSUE_DATE,         
COC_EXPIRATION_DATE,GRADE_TYPE,LIMITATION,EDOC_ID,        
(select disk_path doc_path from iri_edocs where id = Edoc_Id) coc_doc,    
cra_required,   
(select grd.GRADE_code from sicd_grades grd where grd.grade_id = seaOC.grade_id and         
 grd.grade_type='OC'          
and grd.status = 'Active' )GRADE_code,   
(SELECT S.RESTRICTION_NAME    
FROM SICD.SICD_RESTRICTIONS S    
where s.restriction_id = seaOC.restriction and status='Active') restriction_name   
from rmi_seafarer.rmi_seafarer_docs_iface seaOC         
where grade_type = 'OC']]></SQLQuery>
