    <SchemaBasedProperties>
      <LABEL
        ResId="com.rmi.applicationEntry.adfbc.views.readOnlyVO.GetEmailIdForAckView_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="pBatchId"
    Kind="where"
    Type="java.lang.Integer"/>
  <SQLQuery><![CDATA[select bt.esi_batch_id , bt.created_by,   
dtl.ordering_agent_email_id,usr.email_id,   
decode(dtl.ordering_agent_email_id,null,usr.email_id,dtl.ordering_agent_email_id)m_email_id    
from rmi_seafarer.rmi_batch_iface bt, rmi_seafarer.rmi_batch_details dtl ,   
rmi_seafarer.rmi_user usr   
where bt.esi_batch_id = dtl.esi_batch_id(+)   
and bt.created_by = user_id(+)   
and bt.esi_batch_id = :pBatchId]]></SQLQuery>
  <ViewAttribute
    Name="EsiBatchId"
