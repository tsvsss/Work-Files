<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="taskflow_batches_uploadDocument">
    <default-activity>onLoadBatchPendingUploadDocument</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <input-parameter-definition id="__2">
      <name>param_user_id</name>
      <value>#{sessionScope.param_user_id}</value>
      <class>java.lang.Integer</class>
    </input-parameter-definition>
    <managed-bean id="__1">
      <managed-bean-name>BatchesPendingUploadDocBean</managed-bean-name>
      <managed-bean-class>com.rmi.applicationEntry.view.batchesuploaddocumenttaskflow.BatchesPendingUploadDocBean</managed-bean-class>
      <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <view id="BatchesPendingUploadDocument">
      <page>/WEB-INF/com/rmi/applicationEntry/view/batchesuploaddocumenttaskflow/BatchesPendingUploadDocument.jsff</page>
    </view>
    <method-call id="onLoadBatchPendingUploadDocument">
      <method>#{bindings.onLoadBatchPendingUploadDocument.execute}</method>
