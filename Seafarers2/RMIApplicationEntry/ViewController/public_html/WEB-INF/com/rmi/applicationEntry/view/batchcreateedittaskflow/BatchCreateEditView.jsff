                <af:dialog id="d6" type="none"
                           dialogListener="#{pageFlowScope.BatchCreateEditBean.resubmitConfirmationDialogListener}">
                  <f:facet name="buttonBar">
                    <af:toolbar id="t2" inlineStyle="margin-left:200px;">
                      <af:button text="Yes" id="b11" action="back"
                                 actionListener="#{pageFlowScope.BatchCreateEditBean.yesButtonOnConfirmationPopup}"/>
                      <af:button text="No" id="b12"
                                 actionListener="#{pageFlowScope.BatchCreateEditBean.noButtonOnResubmitConfirmation}"/>
                    </af:toolbar>
                  </f:facet>
                  <af:outputText value="Are you sure you want to resubmit the batch?" id="ot7"/>
                </af:dialog>
              </af:popup>
              <af:popup childCreation="deferred" autoCancel="disabled" id="p5"
                        binding="#{pageFlowScope.BatchCreateEditBean.internalAgentCoonfirmationPopup}">
                <af:dialog id="d5" type="yesNo"
                           dialogListener="#{pageFlowScope.BatchCreateEditBean.internalAgentConfirmDialogListener}">
                  <f:facet name="buttonBar"/>
                  <af:outputText value="Are you sure you want to submit the Batch to Internal Agent -II for upload?"
                                 id="ot6"/>
                </af:dialog>
              </af:popup>
            </af:toolbar>
          </f:facet>
          <af:popup childCreation="deferred" autoCancel="disabled" id="p3"
                    binding="#{pageFlowScope.BatchCreateEditBean.submitMessagePopup}">
            <af:dialog id="d3" type="none" contentWidth="280">
              <f:facet name="buttonBar">
                <af:button text="OK" id="b9" action="back"/>
              </f:facet>
              <af:outputText value="Batch has been successfully submitted." id="ot4" inlineStyle="font-size:small;"/>
            </af:dialog>
          </af:popup>
          <af:popup childCreation="deferred" autoCancel="disabled" id="p2"
                    binding="#{pageFlowScope.BatchCreateEditBean.sendToInternalPopup}">
            <af:dialog id="d2" type="none" closeIconVisible="false" contentWidth="700">
              <f:facet name="buttonBar">
                <af:button text="OK" id="b8" action="back"/>
              </f:facet>
              <af:outputText value="Batch has been sent to ‘Orders Pending Upload’ Queue for Internal Agent – II to scan/upload the required documents."
                             id="ot2" inlineStyle="font-size:small;"/>
            </af:dialog>
          </af:popup>
          <af:popup childCreation="deferred" autoCancel="disabled" id="p1" contentDelivery="lazyUncached"
                    binding="#{pageFlowScope.BatchCreateEditBean.validationMessagePopup}">
            <af:dialog id="d1" type="ok">
              <f:facet name="buttonBar"/>
              <af:iterator id="i1" value="#{pageFlowScope.BatchCreateEditBean.message}" var="item">
                <af:panelGroupLayout id="pgl3" layout="vertical">
                  <af:outputText value="{item.value}" id="ot3"/>
                </af:panelGroupLayout>
              </af:iterator>
            </af:dialog>
          </af:popup>
          <af:popup childCreation="deferred" autoCancel="disabled" id="p7" contentDelivery="lazyUncached"
                    binding="#{pageFlowScope.BatchCreateEditBean.messageValidatePopup}">
            <af:dialog id="d7" type="ok" title="Error" titleIconSource="#{resource['images:reddot.png']}">
              <f:facet name="buttonBar"/>
              <af:panelGroupLayout id="pgl7" layout="vertical">
                <af:iterator id="i2" value="#{bindings.ValidationMessageView.collectionModel}" var="row">
                  <af:panelGroupLayout id="pgl8" layout="vertical">
                    <af:spacer width="10" height="10" id="s1"/>
                    <af:outputText value="#{row.ErrorMessage}" id="ot8"
                                   inlineStyle="margin-top:10px; font-weight:bold;"/>
                  </af:panelGroupLayout>
                </af:iterator>
              </af:panelGroupLayout>
              <af:table value="#{bindings.ValidationMessageView.collectionModel}" var="row"
                        rows="#{bindings.ValidationMessageView.rangeSize}"
                        emptyText="#{bindings.ValidationMessageView.viewable ? 'No data to display.' : 'Access Denied.'}"
                        rowBandingInterval="0"
                        selectedRowKeys="#{bindings.ValidationMessageView.collectionModel.selectedRow}"
                        selectionListener="#{bindings.ValidationMessageView.collectionModel.makeCurrent}"
                        rowSelection="single" fetchSize="#{bindings.ValidationMessageView.rangeSize}" id="t3"
                        contentDelivery="immediate" visible="false">
                <af:column headerText="#{bindings.ValidationMessageView.hints.ErrorMessage.label}" id="c1">
                  <af:outputText value="#{row.ErrorMessage}"
                                 shortDesc="#{bindings.ValidationMessageView.hints.ErrorMessage.tooltip}" id="ot9"/>
                </af:column>
              </af:table>
            </af:dialog>
          </af:popup>
          <af:panelLabelAndMessage label="#{bindings.OeHeaderId.hints.label}" id="plam1" visible="false">
            <af:outputText value="#{bindings.OeHeaderId.inputValue}" shortDesc="#{bindings.OeHeaderId.hints.tooltip}"
                           id="ot1">
              <af:convertNumber groupingUsed="false" pattern="#{bindings.OeHeaderId.format}"/>
            </af:outputText>
          </af:panelLabelAndMessage>
          <af:popup childCreation="deferred" autoCancel="disabled" id="p4"
                    binding="#{pageFlowScope.BatchCreateEditBean.resubmitPopupBind}">
            <af:dialog id="d4" type="none" closeIconVisible="false">
              <f:facet name="buttonBar">
                <af:button text="OK" id="b5" action="#{pageFlowScope.BatchCreateEditBean.okButtonOnResubmitButton}"/>
              </f:facet>
              <af:outputText value="Batch has been successfully Resubmitted." id="ot5"/>
            </af:dialog>
          </af:popup>
        </af:panelFormLayout>
      </af:panelGroupLayout>
    </af:panelBox>
  </af:panelGroupLayout>
</jsp:root>
