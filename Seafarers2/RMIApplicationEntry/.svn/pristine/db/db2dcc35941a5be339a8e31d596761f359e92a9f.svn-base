  <SQLQuery><![CDATA[SELECT   hair_color, seafarer_id, last_name, first_name, birth_date,                       
         birth_place, citizenship, nationality, eye_color, gender, height_ft,                       
         height_in, weight_lb, distinguishing_marks,status,PASSPORT_ISSUING_COUNTRY, COUNTRY_OF_RESIDENCE,CITY_OF_RESIDENCE                      
    FROM (SELECT DISTINCT hair_color, seafarer_id, last_name, first_name,                      
                          birth_date, birth_place, citizenship, nationality,                      
                          eye_color, gender, height_ft, height_in, weight_lb,                      
                          distinguishing_marks,                      
                          first_name || ' ' || last_name full_name,status,       
                          PASSPORT_ISSUING_COUNTRY, COUNTRY_OF_RESIDENCE,CITY_OF_RESIDENCE       
                     FROM RMI_SEAFARER_SEARCH_DTL_V                      
                    WHERE seafarer_id =                      
                                   DECODE (:pDocType,                            
                                           NULL, :pDocId,                            
                                           seafarer_id                            
                                          )                            
                      AND NVL (document_number, -99999999) =                            
                             DECODE (:pDocType,                            
                                     NULL, NVL (document_number, -99999999),                            
                                     'FN', NVL (document_number, -99999999),                            
                                     'LN', NVL (document_number, -99999999),                            
                                     'DT', NVL (document_number, -99999999),                            
                                     :pDocId                            
                                    )                           
                      AND NVL(document_type,'Xx') =                            
                             DECODE (:pDocType,                      
                                     'FN', NVL(document_type,'Xx'),                      
                                     'LN', NVL(document_type,'Xx'),                      
                                     'DT', NVL(document_type,'Xx'),                      
                                     NULL, NVL(document_type,'Xx'),                      
                                     :pDocType                      
                                    )                       
                      AND NVL(first_name,'Xx') =                      
                                     DECODE (:pDocType,                      
                                             'FN', :pName,                      
                                             NVL(first_name,'Xx')                      
                                            )                      
                      AND (upper((NVL(last_name,'Xx'))) like upper(DECODE (:pDocType,                      
                                              'LN', decode(:pName,null,null,:pName),                      
                                              NVL(last_name,'Xx')                      
                                             ))                      
                      OR NVL(birth_date,Sysdate-900) =                      
                                 DECODE (:pDocType,                      
                                         'LN', :pDateOfBirth,                      
                                         NVL(birth_date,Sysdate-900)                      
                                        )                
                      OR NVL(seafarer_id,-99999999) =                      
                                 DECODE (:pDocType,                      
                                         'LN', :pSeafarerId,                      
                                         NVL(seafarer_id,-99999999)                      
                                        )                                                                           
                                        )     
                    AND nvl(master_id,-1) = nvl((select max(xpm.id) from xwrl_party_master xpm   
                                      where xpm.source_id = seafarer_id and xpm.source_table = 'SICD_SEAFARERS'),-1))                      
ORDER BY last_name, first_name]]></SQLQuery>
