<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="taskflow_batches_TC">
    <default-activity>onLoadBatchPendingTC</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <input-parameter-definition id="__1">
      <name>param_user_id</name>
      <value>#{sessionScope.param_user_id}</value>
      <class>java.lang.Integer</class>
    </input-parameter-definition>
    <managed-bean id="__4">
      <managed-bean-name>BatchesTCBean</managed-bean-name>
      <managed-bean-class>com.rmi.applicationEntry.view.batchestctaskflow.BatchesTCBean</managed-bean-class>
      <managed-bean-scope>pageFlow</managed-bean-scope>
    </managed-bean>
    <view id="BatchesPendingTC">
      <page>/WEB-INF/com/rmi/applicationEntry/view/batchestctaskflow/BatchesPendingTC.jsff</page>
    </view>
    <method-call id="onLoadBatchPendingTC">
      <method>#{bindings.onLoadBatchPendingTC.execute}</method>
      <outcome>
        <fixed-outcome>onLoadBatchPendingTC</fixed-outcome>
      </outcome>
    </method-call>
    <task-flow-call id="ViewBatchTCTaskFlow1">
      <task-flow-reference>
        <document>/WEB-INF/com/rmi/tradecompapproval/view/viewbatchseafarer/ViewBatchTCTaskFlow.xml</document>
        <id>ViewBatchTCTaskFlow</id>
      </task-flow-reference>
      <input-parameter id="__10">
        <name>p_user_id</name>
        <value>#{sessionScope.param_user_id}</value>
      </input-parameter>
      <input-parameter id="__11">
        <name>p_batch_id</name>
        <value>#{data.com_rmi_applicationEntry_view_BatchesPendingTCPageDef.EsiBatchId.inputValue}</value>
      </input-parameter>
      <input-parameter id="__12">
        <name>p_calling_tf</name>
        <value>myTask</value>
      </input-parameter>
    </task-flow-call>
    <task-flow-call id="SeafarerListTaskFlow">
      <task-flow-reference>
        <document>/WEB-INF/com/rmi/applicationEntry/view/batchestaskflow/SeafarerListTaskFlow.xml</document>
        <id>SeafarerListTaskFlow</id>
      </task-flow-reference>
      <input-parameter id="__5">
        <name>pEsiBatchId</name>
        <value>#{data.com_rmi_applicationEntry_view_BatchesPendingTCPageDef.EsiBatchId1.inputValue}</value>
      </input-parameter>
      <run-as-dialog>
        <display-type>
          <inline-popup/>
        </display-type>
      </run-as-dialog>
    </task-flow-call>
    <task-flow-call id="SeafarerListTcTaskFlow">
      <task-flow-reference>
        <document>/WEB-INF/com/rmi/applicationEntry/view/batchestaskflow/SeafarerListTcTaskFlow.xml</document>
        <id>SeafarerListTcTaskFlow</id>
      </task-flow-reference>
      <input-parameter id="__13">
        <name>pEsiBatchId</name>
        <value>#{data.com_rmi_applicationEntry_view_BatchesPendingTCPageDef.EsiBatchId1.inputValue}</value>
      </input-parameter>
      <run-as-dialog>
        <display-type>
          <inline-popup/>
        </display-type>
      </run-as-dialog>
    </task-flow-call>
    <control-flow-rule id="__2">
      <from-activity-id>onLoadBatchPendingTC</from-activity-id>
      <control-flow-case id="__3">
        <from-outcome>onLoadBatchPendingTC</from-outcome>
        <to-activity-id>BatchesPendingTC</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__7">
      <from-activity-id>BatchesPendingTC</from-activity-id>
      <control-flow-case id="__8">
        <from-outcome>tc</from-outcome>
        <to-activity-id>ViewBatchTCTaskFlow1</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__6">
        <from-outcome>seafarerlist</from-outcome>
        <to-activity-id>SeafarerListTaskFlow</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__9">
        <from-outcome>seafarerlisttc</from-outcome>
        <to-activity-id>SeafarerListTcTaskFlow</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
