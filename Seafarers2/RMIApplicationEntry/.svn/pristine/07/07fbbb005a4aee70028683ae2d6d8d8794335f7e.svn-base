<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="AccountStatusTaskFlow">
    <default-activity>onLoadAccountStatus</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <input-parameter-definition id="__3">
      <name>param_user_id</name>
      <value>#{sessionScope.param_user_id}</value>
      <class>java.lang.Integer</class>
    </input-parameter-definition>
    <managed-bean id="__4">
      <managed-bean-name>AccountStatusBean</managed-bean-name>
      <managed-bean-class>com.rmi.applicationEntry.view.accountstatustaskflow.AccountStatusBean</managed-bean-class>
      <managed-bean-scope>pageFlow</managed-bean-scope>
    </managed-bean>
    <view id="AccountStatus">
      <page>/WEB-INF/com/rmi/applicationEntry/view/accountstatustaskflow/AccountStatus.jsff</page>
    </view>
    <method-call id="onLoadAccountStatus">
      <method>#{bindings.onLoadAccountStatus.execute}</method>
      <outcome>
        <fixed-outcome>onLoadAccountStatus</fixed-outcome>
      </outcome>
    </method-call>
    <control-flow-rule id="__1">
      <from-activity-id>onLoadAccountStatus</from-activity-id>
      <control-flow-case id="__2">
        <from-outcome>onLoadAccountStatus</from-outcome>
        <to-activity-id>AccountStatus</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
