<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="BatchesRejectedView"
  Version="12.1.3.10.47"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="com.rmi.applicationEntry.adfbc.views.readOnlyVO.BatchesRejectedView_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="pUserId"
    Kind="where"
    Type="java.lang.Integer"/>
  <SQLQuery><![CDATA[select esi_batch_id,batch_name,trunc(batch_date) batch_date,batch_status,purchase_order_number,vessel_names,                              
    notes,trunc(batch_submitted_date) batch_submitted_date,                       
    (select count(*) from rmi_seafarer.rmi_seafarers_iface                        
    where rmi_seafarer.rmi_seafarers_iface.esi_batch_id = rmi_seafarer.rmi_batch_iface.esi_batch_id) No_of_seafarer,                       
    (select order_number from OE_ORDER_HEADERS_ALL                     
    where OE_ORDER_HEADERS_ALL.header_id = rmi_seafarer.rmi_batch_iface.oe_header_id) work_order_no,               
    created_by              
    from rmi_seafarer.rmi_batch_iface                                
    where       
    nvl(batch_assigned_to_region,(select default_processing_office from rmi_seafarer.rmi_user     
    where user_id = batch_submitted_by)) = (select default_processing_office from rmi_seafarer.rmi_user where user_id = :pUserId)     
    and         
    (deliver_TO_CUST_ACCOUNT_ID  IN (SELECT ordering_agent_account_id FROM rmi_seafarer.rmi_user_order_agent_map WHERE USER_ID=:pUserId)    
    or (select user_type from rmi_seafarer.rmi_user where user_id = :pUserId) = 'Internal')    
    and    
    ((select user_type from rmi_seafarer.rmi_user where user_id = :pUserId) = (select user_type from rmi_seafarer.rmi_user where user_id = rmi_seafarer.rmi_batch_iface.created_by)   
    or (select user_type from rmi_seafarer.rmi_user where user_id = :pUserId) = 'Internal')    
    and    
     batch_status in ('Submitted','Resubmitted')              
    and (select count(*) from rmi_seafarer.rmi_seafarers_iface sea where sea.seafarer_wf_status in ('Rejected','Unverified')               
    and sea.esi_batch_id = rmi_seafarer.rmi_batch_iface.esi_batch_id) > 0   
    and ((select count(esi_wf_id) from rmi_seafarer.rmi_workflow where last_updated_row = 'Y'   
    and batch_status = 'Deficient' and esi_batch_id = rmi_seafarer.rmi_batch_iface.esi_batch_id 
    and assigned_to is null) >0  
    or  
    (select count(esi_wf_id) from rmi_seafarer.rmi_workflow where last_updated_row = 'Y'   
    and batch_status = 'Deficient' and esi_batch_id = rmi_seafarer.rmi_batch_iface.esi_batch_id)  =0 
    ) 
    order by batch_date asc]]></SQLQuery>
