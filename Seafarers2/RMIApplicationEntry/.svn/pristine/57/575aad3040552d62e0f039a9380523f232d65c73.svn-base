(select first_name||' '||last_name from rmi_seafarer.rmi_user where user_id = rmi_seafarer.rmi_batch_documents_history.document_uploaded_by) uploaded_by_user 
from rmi_seafarer.rmi_batch_documents_history  
where esi_batch_id = :pBatchId   
and seafarer_id = :pSeafarerId  
and doc_code = :pDocCode]]></SQLQuery>
